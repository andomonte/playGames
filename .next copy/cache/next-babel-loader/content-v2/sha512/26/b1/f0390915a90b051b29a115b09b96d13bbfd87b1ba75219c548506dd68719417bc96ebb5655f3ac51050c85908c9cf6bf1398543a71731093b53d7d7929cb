\r\n              />\r\n            )}\r\n            {editar ? (\r\n              <TextField\r\n                id=\"conversoes\"\r\n                label=\"Conversões\"\r\n                variant=\"outlined\"\r\n                className={classes.textField2}\r\n                size=\"small\"\r\n                type=\"number\"\r\n                InputLabelProps={{\r\n                  shrink: true,\r\n                }}\r\n                placeholder=\"\"\r\n              />\r\n            ) : (\r\n              <TextField\r\n                id=\"conversoes\"\r\n                label=\"Conversões:\"\r\n                variant=\"outlined\"\r\n                value={Conversoes}\r\n                disabled\r\n                className={classes.textField2}\r\n                size=\"small\"\r\n                type=\"number\"\r\n                InputLabelProps={{\r\n                  shrink: true,\r\n                }}\r\n                placeholder=\"\"\r\n              />\r\n            )}\r\n            {editar ? (\r\n              <TextField\r\n                id=\"Ofertas\"\r\n                label=\"Ofertas:\"\r\n                variant=\"outlined\"\r\n                className={classes.textField3}\r\n                size=\"small\"\r\n                type=\"string\"\r\n                InputLabelProps={{\r\n                  shrink: true,\r\n                }}\r\n                placeholder=\"\"\r\n                InputProps={{\r\n                  startAdornment: (\r\n                    <InputAdornment position=\"start\">R$</InputAdornment>\r\n                  ),\r\n                }}\r\n              />\r\n            ) : (\r\n              <TextField\r\n                id=\"Ofertas\"\r\n                label=\"Ofertas:\"\r\n                variant=\"outlined\"\r\n                value={Ofertas}\r\n                disabled\r\n                className={classes.textField3}\r\n                size=\"small\"\r\n                type=\"string\"\r\n                Inp