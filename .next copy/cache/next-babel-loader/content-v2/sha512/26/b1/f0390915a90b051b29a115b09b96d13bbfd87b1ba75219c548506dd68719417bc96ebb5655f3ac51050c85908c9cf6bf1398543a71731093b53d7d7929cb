 handleClick = () => {\r\n    if (!editar) {\r\n      setEditar(true);\r\n    } else {\r\n      setEditar(false);\r\n    }\r\n  };\r\n  const url = `/api/consultaDados/${item[0].codigoIgreja}/${Data.slice(3)}`;\r\n  const { data, error } = useSWR(url, fetcher);\r\n  // useSWR('/api/user', (id = 4) => fetcher(id));\r\n  // useSWR('/api/consultaDados', fetcher);\r\n\r\n  if (error) return <div>Failed to load</div>;\r\n  if (!data) return <div>Loading...</div>;\r\n\r\n  const dadosRel = data.filter((val) => val.semana === Semana);\r\n  // console.log(dadosUser.length);\r\n  // console.log(dadosRel[0].data, item, Data, Semana);\r\n  let DataRegistrada = Data;\r\n  let sem = Semana;\r\n  let Adultos = '';\r\n  let Crianças = '';\r\n  let Visitantes = '';\r\n  let Conversoes = '';\r\n  let Ofertas = '';\r\n\r\n  if (dadosRel.length !== 0) {\r\n    // setEditar(false);\r\n    DataRegistrada = dadosRel[0].dataRelatorio;\r\n    Adultos = dadosRel[0].Adultos;\r\n    Crianças = dadosRel[0].criancas;\r\n    Visitantes = dadosRel[0].convidados;\r\n    sem = dadosRel[0].semana;\r\n    Conversoes = dadosRel[0].conversoes;\r\n    Ofertas = dadosRel[0].Ofertas;\r\n  }\r\n  // const dia = Data.getDate().toString();\r\n  // console.log(Data.slice(3)); // retorna 'Rica'\r\n  //  console.log(Data, { sem });\r\n  //--------------------------------------------------------------------------\r\n\r\n  //--------------------------------------------------------------------------\r\n  const valid = (props) => {\r\n    if (!adultos) {\r\n      setValidarAdultos('nao');\r\n    } else {\r\n      setValidarAdultos('sim');\r\n    }\r\n    if (!criancas) {\r\n      setValidarCriancas('nao');\r\n    }\r\n  };\r\n  //--------------------------------------------------------------------------\r\n  //--------------------------------------------------------------------------\r\n\r\n  const submitData = async (e) => {\r\n    e.preventDefault();\r\n    const valida = await valid();\r\n    console.log(valida);\r\n    if (valida)\r\n      try {\r\n        const body = {\r\n          igreja,\r\n          codigoIgreja,\r\n          semana,\r\n          dataRelatorio,\r\n          adultos,\r\n          criancas,\r\n          convidados,\r\n          conversoes,\r\n        };\r\n        let urlCreate = '';\r\n        if (dadosRel.length === 0) {\r\n          urlCreate = `${window.location.origin}/api/criarRelatorio`;\r\n        } else {\r\n          urlCreate = `${window.location.origin}/api/updateRelatorio`;\r\n        }\r\n        await fetch(urlCreate, {\r\n          method: 'POST',\r\n          headers: { 'Content-Type': 'application/json' },\r\n          body: JSON.stringify(body),\r\n        });\r\n        console.log('dados criados', fetch);\r\n        // await Router.push('/drafts');\r\n      } catch (errors) {\r\n        console.error(errors);\r\n      }\r\n  };\r\n  return (\r\n    <Box\r\n      mt={3}\r\n      className={classes.box}\r\n      width=\"100%\"\r\n      //            maxWidth={1200}\r\n      height=\"auto\"\r\n      borderRadius={16}\r\n    >\r\n      <form noValidate autoComplete=\"off\" width=\"100%\" className={classes.root}>\r\n        <Grid container spacing={3}>\r\n          <Grid item xs={12} md={12} lg={12} xl={12}>\r\n            <TextField\r\n              id=\"igreja\"\r\n              label=\"Igreja\"\r\n              variant=\"outlined\"\r\n              value={item[0].igreja}\r\n              disabled\r\n              size=\"small\"\r\n              className={classes.textField1}\r\n            />\r\n\r\n            <TextField\r\n              id=\"codigoIgreja\"\r\n              label=\"Código da Igreja\"\r\n              variant=\"outlined\"\r\n              value={item[0].codigoIgreja}\r\n              disabled\r\n              className={classes.textField2}\r\n              size=\"small\"\r\n            />\r\n            <TextField\r\n              id=\"data\"\r\n              label=\"Data do Relatório\"\r\n              variant=\"ou