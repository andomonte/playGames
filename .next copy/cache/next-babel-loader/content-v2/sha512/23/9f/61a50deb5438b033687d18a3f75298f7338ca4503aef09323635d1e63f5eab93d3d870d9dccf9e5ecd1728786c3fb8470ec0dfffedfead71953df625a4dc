ons/Edit';\r\nimport InputAdornment from '@material-ui/core/InputAdornment';\r\nimport SaveIcon from '@material-ui/icons/Save';\r\nimport ReplyRoundedIcon from '@material-ui/icons/ReplyRounded';\r\nimport { yellow } from '@material-ui/core/colors';\r\n\r\nconst fetcher = (url) => axios.get(url).then((res) => res.data);\r\n// const fetcher = (url) => fetch(url).then((r) => r.json());\r\n\r\nconst useStyles = makeStyles((theme) => ({\r\n  root: {\r\n    alignContent: 'center',\r\n  },\r\n  buttonCancel: {\r\n    alignContent: 'center',\r\n    // color: theme.palette.background.primary,\r\n    color: theme.palette.getContrastText(yellow[500]),\r\n    backgroundColor: yellow[500],\r\n    '&:hover': {\r\n      backgroundColor: yellow[700],\r\n    },\r\n  },\r\n  button: {\r\n    alignContent: 'center',\r\n  },\r\n  box: {\r\n    '& > *': {\r\n      margin: theme.spacing(2),\r\n      //  width: '50ch',\r\n    },\r\n    display: 'flex',\r\n    alignItems: 'center',\r\n    alignContent: 'center',\r\n    justifyContent: 'center',\r\n  },\r\n  textField1: {\r\n    marginLeft: theme.spacing(1),\r\n    marginRight: theme.spacing(1),\r\n    width: '46ch',\r\n    [theme.breakpoints.down('md')]: {\r\n      width: '50ch',\r\n    },\r\n  },\r\n  textField2: {\r\n    marginLeft: theme.spacing(1),\r\n    marginRight: theme.spacing(1),\r\n    width: '16ch',\r\n    alignItems: 'center',\r\n  },\r\n  textField3: {\r\n    marginLeft: theme.spacing(1),\r\n    marginRight: theme.spacing(1),\r\n    width: '12ch',\r\n    alignItems: 'center',\r\n  },\r\n  textField4: {\r\n    marginLeft: theme.spacing(1),\r\n    marginRight: theme.spacing(1),\r\n    width: '18ch',\r\n    alignItems: 'center',\r\n  },\r\n  textField22: {\r\n    marginLeft: theme.spacing(1),\r\n    marginRight: theme.spacing(1),\r\n    width: '22ch',\r\n    alignItems: 'center',\r\n  },\r\n}));\r\n\r\nfunction formulario({ item, secao, Data, Semana }) {\r\n  const classes = useStyles();\r\n  // const [editar, setEditar] = React.useState(true);\r\n  const [igreja] = React.useState(item[0].igreja);\r\n  const [codigoIgreja] = React.useState(item[0].codigoIgreja);\r\n  const [semana] = React.useState(Semana);\r\n  const [dataRelatorio] = React.useState(Data);\r\n  const [adultos, setAdultos] = React.useState('');\r\n  const [criancas, setCriancas] = React.useState('');\r\n  const [visitantes, setVisitantes] = React.useState('');\r\n  const [conversoes, setConversoes] = React.useState('');\r\n  const [ofertas, setOfertas] = React.useState('');\r\n  const [validarAdultos, setValidarAdultos] = React.useState('sim');\r\n  const [validarCriancas, setValidarCriancas] = React.useState('sim');\r\n  const [validarVisitantes, setValidarVisitantes] = React.useState('sim');\r\n  const [validarConversoes, setValidarConversoes] = React.useState('sim');\r\n  const [validarOfertas, setValidarOfertas] = React.useState('sim');\r\n  const mes = String(Number(Data.slice(3, 5)));\r\n  const ano = Data.slice(6, 10);\r\n  const [editar, setEditar] = React.useState();\r\n\r\n  const handleClick = () => {\r\n    if (!editar) setEditar(true);\r\n    else {\r\n      setEditar(false);\r\n    }\r\n    console.log('vai pessoal', editar);\r\n  };\r\n\r\n  //----------------------------------------------------------------------\r\n  const url = `/api/consultaDados/${item[0].codigoIgreja}/${mes}/${ano}`;\r\n  const { data, error } = useSWR(url, fetcher);\r\n  // useSWR('/api/user', (id = 4) => fetcher(id));\r\n  // useSWR('/api/consultaDados', fetcher);\r\n  if (error) return <div>Failed to load</div>;\r\n  if (!data) return <div>Loading...</div>;\r\n  //---------------------------------------------------------------------------\r\n  const dadosRel = data.filter((val) => val.semana === Semana);\r\n  // console.log(dadosRel.length, data);\r\n  // console.log(dadosRel[0].data, item, Data, Semana);\r\n  let DataRelatorio = Data;\r\n  let sem = Semana;\r\n  let Adultos = '';\r\n  let Crianças = '';\r\n  let Visitantes = '';\r\n  let Conversoes = '';\r\n  let Ofertas = '';\r\n\r\n  if (dadosRel.length !== 0) {\r\n    // setEditar(false);\r\n    // setEditar(false);\r\n    DataRelatorio = dadosRel[0].dataRelatorio;\r\n    Adultos = dadosRel[0].adultos;\r\n    Crianças = dadosRel[0].criancas;\r\n    Visitantes = dadosRel[0].visitantes;\r\n    sem = dadosRel[0].semana;\r\n    Conversoes = dadosRel[0].conversoes;\r\n    Ofertas = dadosRel[0].ofertas;\r\n  }\r\n\r\n  // const dia = Data.getDate().toString();\r\n  // console.log(Data.slice(3)); // retorna 'Rica'\r\n  //  console.log(Data, { sem });\r\n  //--------------------------------------------------------------------------\r\n\r\n  //--------------------------------------------------------------------------\r\n  const valid = () => {\r\n    console.log({ adulto });\r\n    if (!adultos || !criancas || !visitantes || !conversoes || !ofertas) {\r\n      return false;\r\n    }\r\n    return true;\r\n  };\r\n  //--------------------------------------------------------------------------\r\n  //--------------------------------------------------------------------------\r\n\r\n  const submitData = async (e) => {\r\n    e.preventDefault();\r\n    const valida = valid();\r\n    console.log('entrou no submit:', valida);\r\n    // const dataRelatorio = DataRelatorio;\r\n\r\n    if (valida) {\r\n      try {\r\n        const body = {\r\n          igreja,\r\n          codigoIgreja,\r\n          semana,\r\n          mes,\r\n          ano,\r\n          adultos,\r\n          criancas,\r\n          visitantes,\r\n          conversoes,\r\n          dataRelatorio,\r\n          ofertas,\r\n        };\r\n        console.log('dados enviados:', body);\r\n        let urlCreate = '';\r\n        if (dadosRel.length === 0) {\r\n          urlCreate = `${window.location.origin}/api/criarRelatorio`;\r\n        } else {\r\n          urlCreate = `${window.location.origin}/api/updateRelatorio`;\r\n        }\r\n        await fetch(urlCreate, {\r\n          method: 'POST',\r\n          headers: { 'Content-Type': 'application/json' },\r\n          body: JSON.stringify(body),\r\n        });\r\n        //    console.log('dados criados', fetch);\r\n        // await Router.push('/drafts');\r\n      } catch (errors) {\r\n        //     console.error(errors);\r\n      }\r\n    }\r\n  };\r\n  return (\r\n    <Box\r\n      mt={3}\r\n      className={classes.box}\r\n      width=\"100%\"\r\n      //            maxWidth={1200}\r\n      height=\"auto\"\r\n      borderRadius={16}\r\n    >\r\n      <form noValidate autoComplete=\"off\" width=\"100%\" className={classes.root}>\r\n        <Grid container spacing={3}>\r\n          <Grid item xs={12} md={12} lg={12} xl={12}>\r\n            <TextField\r\n              id=\"igreja\"\r\n              label=\"Igreja\"\r\n              variant=\"outlined\"\r\n              value={item[0].igreja}\r\n              disabled\r\n              size=\"small\"\r\n              className={classes.textField1}\r\n            />\r\n\r\n            <TextField\r\n              id=\"codigoIgreja\"\r\n              label=\"Código da Igreja\"\r\n              variant=\"outlined\"\r\n              value={item[0].codigoIgreja}\r\n              disabled\r\n              className={classes.textField2}\r\n              size=\"small\"\r\n            />\r\n            <TextField\r\n              id=\"data\"\r\n              label=\"Data do Relatório\"\r\n              variant=\"outlined\"\r\n              value={DataRelatorio}\r\n              disabled\r\n              className={classes.textField2}\r\n              size=\"small\"\r\n            />\r\n\r\n            <TextField\r\n              id=\"Semana\"\r\n              label=\"Semana Nº:\"\r\n              variant=\"outlined\"\r\n              value={sem}\r\n              disabled\r\n              className={classes.textField3}\r\n              size=\"small\"\r\n            />\r\n          </Grid>\r\n        </Grid>\r\n        <br />\r\n        <br />\r\n        <Grid container 