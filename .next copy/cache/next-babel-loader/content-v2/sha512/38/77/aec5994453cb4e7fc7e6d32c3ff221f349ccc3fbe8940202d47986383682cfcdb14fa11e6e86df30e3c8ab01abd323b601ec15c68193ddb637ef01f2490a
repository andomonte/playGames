\n          urlCreate = `${window.location.origin}/api/updateRelatorio/${ids}`;\r\n        }\r\n        // console.log('endereço usr:', urlCreate);\r\n\r\n        await fetch(urlCreate, {\r\n          method: 'POST',\r\n          headers: { 'Content-Type': 'application/json' },\r\n          body: JSON.stringify(body),\r\n        });\r\n        setOpenDialog(true);\r\n        setEditar(false);\r\n      } catch (errors) {}\r\n    }\r\n  };\r\n  return (\r\n    <Box\r\n      mt={3}\r\n      className={classes.box}\r\n      width=\"100%\"\r\n      //            maxWidth={1200}\r\n      height=\"auto\"\r\n      borderRadius={16}\r\n    >\r\n      <form noValidate autoComplete=\"off\" width=\"100%\" className={classes.root}>\r\n        <Grid container spacing={3}>\r\n          <Grid item xs={12} md={12} lg={12} xl={12}>\r\n            <TextField\r\n              id=\"igreja\"\r\n              label=\"Igreja\"\r\n              variant=\"outlined\"\r\n              value={item[0].igreja}\r\n              disabled\r\n              size=\"small\"\r\n              className={classes.textField1}\r\n            />\r\n\r\n            <TextField\r\n              id=\"codigoIgreja\"\r\n              label=\"Código da Igreja\"\r\n              variant=\"outlined\"\r\n              value={item[0].codigoIgreja}\r\n              disabled\r\n              className={classes.textField2}\r\n              size=\"small\"\r\n            />\r\n            <TextField\r\n              id=\"data\"\r\n              label=\"Data do Relatório\"\r\n              variant=\"outlined\"\r\n              value={DataRelatorio}\r\n              disabled\r\n              className={classes.textField2}\r\n              size=\"small\"\r\n            />\r\n\r\n            <TextField\r\n              id=\"Semana\"\r\n              label=\"Semana Nº:\"\r\n              variant=\"outlined\"\r\n              value={sem}\r\n              disabled\r\n              className={classes.textField3}\r\n              size