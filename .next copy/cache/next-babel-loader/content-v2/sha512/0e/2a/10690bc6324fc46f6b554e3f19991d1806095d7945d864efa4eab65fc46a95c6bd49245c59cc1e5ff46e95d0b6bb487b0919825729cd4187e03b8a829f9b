                    </Button>\r\n                  )}\r\n                </Box>\r\n              )}\r\n              {loading && (\r\n                <CircularProgress\r\n                  size={24}\r\n                  className={classes.buttonProgress}\r\n                />\r\n              )}\r\n            </form>\r\n          </Hidden>\r\n          <Hidden mdUp>\r\n            <form\r\n              noValidate\r\n              autoComplete=\"off\"\r\n              width=\"100%\"\r\n              className={classes.root}\r\n            >\r\n              <Grid item xs={12}>\r\n                <Box className={classes.novoBox}>\r\n                  <TextField\r\n                    id=\"igreja\"\r\n                    label=\"Igreja\"\r\n                    variant=\"outlined\"\r\n                    value={item[0].igreja}\r\n                    disabled\r\n                    size=\"small\"\r\n                    className={classes.tf_m}\r\n                  />\r\n                </Box>\r\n              </Grid>\r\n              <Box flexDirection=\"row\" display={{ xs: 'none', sm: 'block' }}>\r\n                <Grid item xs={12}>\r\n                  <Box className={classes.novoBox}>\r\n                    <TextField\r\n                      id=\"codigoIgreja\"\r\n                      label=\"Código\"\r\n                      variant=\"outlined\"\r\n                      value={item[0].codigoIgreja}\r\n                      disabled\r\n                      className={classes.tf_m}\r\n                      size=\"small\"\r\n                    />\r\n                  </Box>\r\n                </Grid>\r\n              </Box>\r\n              <Box display=\"flex\" flexDirection=\"row\">\r\n                <Grid item xs={7}>\r\n                  <Box className={classes.novoBox}>\r\n                    {dadosRel.length !== 0 ? (\r\n                      <TextField\r\n                        id=\"data\"\r\n                        label=\"Data\"\r\n                  