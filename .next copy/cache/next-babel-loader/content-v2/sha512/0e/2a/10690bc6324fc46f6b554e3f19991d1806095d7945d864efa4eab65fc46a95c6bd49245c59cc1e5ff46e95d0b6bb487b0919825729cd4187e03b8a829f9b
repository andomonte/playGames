ltos,\r\n          criancas,\r\n          visitantes,\r\n          conversoes,\r\n          dataRelatorio,\r\n          ofertas,\r\n        };\r\n\r\n        let urlCreate = '';\r\n        if (dadosRel.length === 0) {\r\n          urlCreate = `${window.location.origin}/api/criarRelatorio`;\r\n        } else {\r\n          urlCreate = `${window.location.origin}/api/updateRelatorio/${ids}`;\r\n        }\r\n\r\n        await fetch(urlCreate, {\r\n          method: 'POST',\r\n          headers: { 'Content-Type': 'application/json' },\r\n          body: JSON.stringify(body),\r\n        });\r\n        setLoading(false);\r\n        setEditar(false);\r\n        mutate(url);\r\n      } catch (errors) {\r\n        console.errors();\r\n      }\r\n    }\r\n  };\r\n  return (\r\n    <>\r\n      {session ? (\r\n        <Box\r\n          mt={3}\r\n          className={classes.box}\r\n          width=\"100%\"\r\n          //            maxWidth={1200}\r\n          height=\"auto\"\r\n          borderRadius={16}\r\n        >\r\n          <Hidden smDown>\r\n            <form\r\n              noValidate\r\n              autoComplete=\"off\"\r\n              width=\"100%\"\r\n              className={classes.root}\r\n            >\r\n              <TextField\r\n                id=\"igreja\"\r\n                label=\"Igreja\"\r\n                variant=\"outlined\"\r\n                value={item[0].igreja}\r\n                disabled\r\n                size=\"small\"\r\n                className={classes.tf_12}\r\n              />\r\n\r\n              <TextField\r\n                id=\"codigoIgreja\"\r\n                label=\"Código da Igreja\"\r\n                variant=\"outlined\"\r\n                value={item[0].codigoIgreja}\r\n                disabled\r\n                className={classes.tf_4}\r\n                size=\"small\"\r\n              />\r\n              {dadosRel.length !== 0 ? (\r\n                <TextField\r\n                  id=\"data\"\r\n                  label