tem])).values(),\r\n    ];\r\n    // console.log('deu certo:', newValue);\r\n  };\r\n\r\n  //= =========================================================================\r\n  const updateFile = (id, data) => {\r\n    newList = newValue.map((uploadedFile) =>\r\n      id === uploadedFile.id ? { ...uploadedFile, ...data } : uploadedFile,\r\n    );\r\n    // console.log('newlist', newList);\r\n  };\r\n  const processUpload = (uploadedFile) => {\r\n    const data = new FormData();\r\n    data.append('file', uploadedFile.file, uploadedFile.name);\r\n\r\n    api.post('api/fotos', data, {\r\n      onUploadProgress: (e) => {\r\n        const progress = Math.round((e.loaded * 100) / e.total);\r\n        updateFile(uploadedFile.id, { progress });\r\n      },\r\n    });\r\n  };\r\n\r\n  const iniciarEnvio = () => {\r\n    newValue.forEach(processUpload);\r\n  };\r\n  useEffect(() => {\r\n    handleUpload();\r\n    setLista(lista + 1);\r\n    if (lista >= 5) setLista(0);\r\n    setarray([...array, files]);\r\n    console.log(lista, array);\r\n    // setLista(newList);\r\n    // iniciarEnvio();\r\n  }, [files]);\r\n\r\n  //= =========================================================================\r\n\r\n  // clean up\r\n  const DragMessage = (a, b) => {\r\n    if (!a) {\r\n      return <UploadMessage>Arraste arquivos aqui..</UploadMessage>;\r\n    }\r\n    if (b) {\r\n      return (\r\n        <UploadMessage type=\"error\">Arquivo não suportado..</UploadMessage>\r\n      );\r\n    }\r\n    return (\r\n      <UploadMessage type=\"success\">Solte os arquivos aqui..</UploadMessage>\r\n    );\r\n  };\r\n  return (\r\n    <section>\r\n      <DropContainer\r\n        {...getRootProps()}\r\n        isDragActive={isDragActive}\r\n        isDragReject={isDragReject}\r\n      >\r\n        <input {...getInputProps()} />\r\n        {DragMessage(isDragActive, isDragReject)}\r\n      </DropContainer>\r\n      {novoValor(state)}\r\n      {iniciarEnvio()}\r\n      <aside>\r\n        {array.length > 0 && <FileList files={array} arquivo={files[0]} />}\r\n        {thumbs}\r\n      </aside>\r\n    </section>\r\n  );\r\n}\r\n\r\nexport default DropzoneComponent;\r\n"]},"metadata":{},"sourceType":"module"}