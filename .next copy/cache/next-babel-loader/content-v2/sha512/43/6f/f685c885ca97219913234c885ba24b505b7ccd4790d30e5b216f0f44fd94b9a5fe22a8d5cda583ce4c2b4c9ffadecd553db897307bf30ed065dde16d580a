                  shrink: true,\r\n                }}\r\n                variant=\"outlined\"\r\n                placeholder=\"0\"\r\n                size=\"small\"\r\n              />\r\n            ) : (\r\n              <TextField\r\n                className={classes.textField4}\r\n                id=\"adultos\"\r\n                label=\"Membros Adultos\"\r\n                type=\"number\"\r\n                InputLabelProps={{\r\n                  shrink: true,\r\n                }}\r\n                value={adultos}\r\n                variant=\"outlined\"\r\n                placeholder=\"0\"\r\n                size=\"small\"\r\n                disabled\r\n                inputRef={(input) => input && input.focus()}\r\n              />\r\n            )}\r\n\r\n            {editar ? (\r\n              <TextField\r\n                className={classes.textField4}\r\n                id=\"crianças\"\r\n                label=\"Membros Crianças\"\r\n                type=\"number\"\r\n                InputLabelProps={{\r\n                  shrink: true,\r\n                }}\r\n                variant=\"outlined\"\r\n                placeholder=\"0\"\r\n                size=\"small\"\r\n              />\r\n            ) : (\r\n              <TextField\r\n                className={classes.textField4}\r\n                id=\"crianças\"\r\n                label=\"Membros Crianças\"\r\n                type=\"number\"\r\n                InputLabelProps={{\r\n                  shrink: true,\r\n                }}\r\n                value={crianças}\r\n                variant=\"outlined\"\r\n                placeholder=\"0\"\r\n                size=\"small\"\r\n                disabled\r\n              />\r\n            )}\r\n\r\n            {editar ? (\r\n              <TextField\r\n                className={classes.textField2}\r\n                id=\"visitantes\"\r\n                label=\"Visitantes\"\r\n                type=\"number\"\r\n                InputLabelProps={{\r\n                  shrink: true,\r\n                }}\r\n                variant=\"outlined\"\r\n                placeholder=\"0\"\r\n                size=\"small\"\r\n              />\r\n            ) : (\r\n              <TextField\r\n                className={classes.textField2}\r\n                id=\"visitantes\"\r\n                label=\"Visitantes\"\r\n                type=\"number\"\r\n                InputLabelProps={{\r\n                  shrink: true,\r\n                }}\r\n                value={visitantes}\r\n                variant=\"outlined\"\r\n                placeholder=\"0\"\r\n                size=\"small\"\r\n                disabled\r\n              />\r\n            )}\r\n\r\n            <TextField\r\n              id=\"semana\"\r\n              label=\"Semana Nº:\"\r\n              variant=\"outlined\"\r\n              value={sem}\r\n              disabled\r\n              className={classes.textField2}\r\n              size=\"small\"\r\n            />\r\n          </Grid>\r\n        </Grid>\r\n        <br />\r\n        <br />\r\n        <Button\r\n          variant=\"contained\"\r\n          color=\"primary\"\r\n          size=\"small\"\r\n          className={classes.button}\r\n          startIcon={<EditIcon />}\r\n          onClick={handleClick}\r\n          mt={3}\r\n\r\n          //  startIcon={<SaveIcon />}\r\n        >\r\n          Editar\r\n        </Button>\r\n      </form>\r\n    </Box>\r\n  );\r\n}\r\n\r\nexport default formulario;\r\n"]},"metadata":{},"sourceType":"module"}