n: 10,\r\n      width: '205px',\r\n    },\r\n  },\r\n  tf_4: {\r\n    margin: 10,\r\n    // marginRight: 8,\r\n    width: '155px',\r\n    textAlign: 'center', // alignItems: 'center',\r\n    [theme.breakpoints.down('md')]: {\r\n      marginLeft: 10,\r\n      width: '130px',\r\n    },\r\n  },\r\n  tf_3: {\r\n    margin: 10,\r\n    textAlign: 'center',\r\n    // marginRight: 8,\r\n    width: '120px',\r\n    // alignItems: 'center',\r\n    [theme.breakpoints.down('md')]: {\r\n      marginLeft: 10,\r\n      width: '110px',\r\n    },\r\n  },\r\n  buttonProgress: {\r\n    color: green[500],\r\n    position: 'absolute',\r\n    top: '50%',\r\n    left: '50%',\r\n    marginTop: -12,\r\n    marginLeft: -12,\r\n  },\r\n}));\r\n\r\nfunction formulario({ item, Data, Semana }) {\r\n  const classes = useStyles();\r\n  const [open, setOpen] = React.useState(false);\r\n  const uploadedFiles = [];\r\n  const [fileObjects, setFileObjects] = React.useState([]);\r\n  const dataEvento = String(Data.split('/').join('')); // retira a / da data\r\n\r\n  const [session] = useSession();\r\n\r\n  const [editar, setEditar] = React.useState();\r\n  const [igreja] = React.useState(item[0].igreja);\r\n  const [codigoIgreja] = React.useState(item[0].codigoIgreja);\r\n  const [evento, setEvento] = React.useState('');\r\n  const [adultos, setAdultos] = React.useState('');\r\n  const [criancas, setCriancas] = React.useState('');\r\n  const [visitantes, setVisitantes] = React.useState('');\r\n  const [conversoes, setConversoes] = React.useState('');\r\n  const [descriçao, setDescriçao] = React.useState('');\r\n  const [img01, setImg01] = React.useState('');\r\n  const [validarEvento, setValidarEvento] = React.useState('sim');\r\n  const [validarAdultos, setValidarAdultos] = React.useState('sim');\r\n  const [validarCriancas, setValidarCriancas] = React.useState('sim');\r\n  const [validarVisitantes, setValidarVisitantes] = React.useState('sim');\r\n  const [validarConversoes, setValidarConversoes] = React.useState('sim');\r\n  const [validarDescriçao, setValidarDescriçao] = React.useState('sim');\r\n  let Img01 = '';\r\n  const [contador, setContador] = React.useState(0);\r\n  const [loading, setLoading] = React.useState(false);\r\n\r\n  //----------------------------------------------------------------------\r\n  const url = `${window.location.origin}/api/consultaEventos/${item[0].codigoIgreja}/${dataEvento}`;\r\n\r\n  function atualizarLista(idAtual) {\r\n    setFileObjects(fileObjects.filter((el) => el.id !== idAtual.id));\r\n\r\n    //    setFileObjects(uploadedFiles);\r\n  }\r\n\r\n  const FileList = ({ files }) => (\r\n    <div>\r\n      <Container>\r\n        {files.map((uploadedFile) => (\r\n          <li key={uniqueId()}>\r\n            <FileInfo>\r\n              <Preview src={uploadedFile.preview} />\r\n              <Box className={classes.qtyLetras}>\r\n                <strong>{uploadedFile.name}</strong>\r\n                <span>{uploadedFile.readableSize}</span>\r\n              </Box>\r\n            </FileInfo>\r\n            <div>\r\n              {uploadedFile.progress > 0 && !uploadedFile.error ? (\r\n                <CircularProgressbar\r\n                  styles={{\r\n                    root: { width: 30 },\r\n                    path: { stroke: '#7159c1' },\r\n                  }}\r\n                  strokeWidth={10}\r\n                  // percentage={20} // {uploadedFile.progress}\r\n\r\n                  value={uploadedFile.progress}\r\n                />\r\n              ) : (\r\n                <DeleteForeverIcon\r\n                  type=\"button\"\r\n                  onClick={() => {\r\n                    atualizarLista(uploadedFile);\r\n                  }}\r\n                  style={{ color: 'red', fontSize: 30 }}\r\n                />\r\n              )}\r\n              {uploadedFile.url && (\r\n                <a\r\n                  href=\"https://sistemaidpb.s3.amazonaws.com/CAMPO%20GRANDE.png\"\r\n                  target=\"_blank\"\r\n                  rel=\"noopener noreferrer\"\r\n                >\r\n                  <MdLink styles={{ marginRight: 8 }} size={24} color=\"#222\" />\r\n                </a>\r\n              )}\r\n              {uploadedFile.uploaded && (\r\n                <MdCheckCircle size={24} color=\"#78e5d5\" />\r\n              )}\r\n              {uploadedFile.error && <MdError size={24} color=\"#e57878\" />}\r\n            </div>\r\n          </li>\r\n        ))}\r\n      </Container>\r\n    </div>\r\n  );\r\n  const handleOpen = () => {\r\n    setOpen(true);\r\n  };\r\n  const handleVoltar = (e) => {\r\n    setFileObjects(fileObjects.slice(fileObjects.indexOf(e.target.name, 1)));\r\n    setFileObjects(fileObjects.filter((x) => x % 2));\r\n    setOpen(false);\r\n  };\r\n\r\n  const handleClose = () => {\r\n    setOpen(false);\r\n  };\r\n  const fileSend = () => {\r\n    /*  for (let i = 0; i < 5; i += 1) {\r\n      uploadedFiles[i] = [fileObjects[i]];\r\n    } */\r\n    Img01 = JSON.parse(JSON.stringify(fileObjects[0]));\r\n    setOpen(false);\r\n  };\r\n\r\n  function MyDropzone() {\r\n    const onDrop = useCallback((acceptedFiles) => {\r\n      setFileObjects(\r\n        acceptedFiles.map((file) =>\r\n          Object.assign(file, {\r\n            preview: URL.createObjectURL(file),\r\n            id: uniqueId(),\r\n            readableSize: filesize(file.size),\r\n            progress: 0,\r\n            uploaded: false,\r\n            error: false,\r\n            url: null,\r\n          }),\r\n        ),\r\n      );\r\n      setFileObjects([].concat(fileObjects, acceptedFiles));\r\n    });\r\n\r\n    const {\r\n      getRootProps,\r\n      getInputProps,\r\n      isDragActive,\r\n      isDragReject,\r\n    } = useDropzone({\r\n      onDrop,\r\n\r\n      accept: 'image/*',\r\n    });\r\n\r\n    const DragMessage = (a, b) => {\r\n      if (!a) {\r\n        return (\r\n          <UploadMessage>\r\n            <Button>\r\n              <ImageSearchIcon fontSize=\"large\" style={{ color: 'black' }} />\r\n            </Button>\r\n            Pressione Aqui para Inseir Imagens\r\n          </UploadMessage>\r\n        );\r\n      }\r\n      if (b) {\r\n        return (\r\n          <UploadMessage type=\"error\">Arquivo não suportado..</UploadMessage>\r\n        );\r\n      }\r\n      return (\r\n        <UploadMessage type=\"success\">Solte os arquivos aqui..</UploadMessage>\r\n      );\r\n    };\r\n\r\n    return (\r\n      <>\r\n        <Dropzone>\r\n          {() => (\r\n            <DropContainer\r\n              {...getRootProps()}\r\n              isDragActive={isDragActive}\r\n              isDragReject={isDragReject}\r\n            >\r\n              <input {...getInputProps()} />\r\n              {DragMessage(isDragActive, isDragReject)}\r\n            </DropContainer>\r\n          )}\r\n        </Dropzone>\r\n\r\n        {fileObjects.length > 0 && <FileList files={fileObjects} />}\r\n        {fileObjects.length > 4 && fileSend()}\r\n        <Box display=\"flex\" flexDirection=\"row\" justifyContent=\"center\">\r\n          <Button\r\n            variant=\"contained\"\r\n            size=\"small\"\r\n            color=\"secondary\"\r\n            onClick={handleVoltar}\r\n          >\r\n            Cancelar\r\n          </Button>\r\n          <Button\r\n            variant=\"contained\"\r\n            size=\"small\"\r\n            color=\"secondary\"\r\n            onClick={handleVoltar}\r\n          >\r\n            Salvar\r\n          </Button>\r\n        </Box>\r\n      </>\r\n    );\r\n  }\r\n\r\n  // const [editar, setEditar] = React.useState(true);\r\n\r\n  const { data, error } = useSWR(url, fetcher);\r\n  // useSWR('/api/user', (id = 4) => fetcher(id));\r\n  // useSWR('/api/consultaDados', fetcher);\r\n  if (error) return <div>Failed to load</div>;\r\n  if (!data) return <div>Loading...</div>;\r\n  //---------------------------------------------------------------------------\r\n  const dadosRel = data.filter((val) => val.semana === Semana);\r\n  let Evento = '';\r\n  let Adultos = '';\r\n  let Crianças = '';\r\n  let Visitantes = '';\r\n  let Conversoes = '';\r\n  let ids = '';\r\n  const Descriçao = '';\r\n\r\n  if (dadosRel.length !== 0) {\r\n    Evento = dadosRel[0].evento;\r\n    Adultos = dadosRel[0].adultos;\r\n    Crianças = dadosRel[0].criancas;\r\n    Visitantes = dadosRel[0].visitantes;\r\n    Conversoes = dadosRel[0].conversoes;\r\n    ids = dadosRel[0].id;\r\n  }\r\n  //  if (uploadedFiles.length > 0)\r\n  // Img01 = JSON.parse(JSON.stringify(uploadedFiles[0])); // uploadedFiles[0].preview;\r\n\r\n  //--------------------------------------------------------------------------\r\n  //--------------------------------------------------------------------------\r\n  const valid = () => {\r\n    if (!adultos || !criancas || !visitantes || !conversoes || !evento) {\r\n      return false;\r\n    }\r\n    return true;\r\n  };\r\n  //--------------------------------------------------------------------------\r\n  const handleClick = () => {\r\n    setEvento(Evento);\r\n    setAdultos(Adultos);\r\n    setCriancas(Crianças);\r\n    setVisitantes(Visitantes);\r\n    setConversoes(Conversoes);\r\n    setDescriçao(Descriçao);\r\n    console.log(Img01); // uploadedFiles[0].preview);\r\n    setImg01(Img01);\r\n    if (!editar) {\r\n      setEditar(true);\r\n      setContador('0');\r\n    } else {\r\n      setEditar(false);\r\n    }\r\n  };\r\n  //--------------------------------------------------------------------------\r\n  //--------------------------------------------------------------------------\r\n\r\n  const submitData = async (e) => {\r\n    e.preventDefault();\r\n    const valida = valid();\r\n    setLoading(true);\r\n\r\n    if (valida) {\r\n      try {\r\n        const body = {\r\n          evento,\r\n          igreja,\r\n          codigoIgreja,\r\n          dataEvento,\r\n          adultos,\r\n          criancas,\r\n          visitantes,\r\n          conversoes,\r\n          img01,\r\n          img02,\r\n          img03,\r\n          img04,\r\n          img05,\r\n        };\r\n\r\n        let urlCreate = '';\r\n        if (dadosRel.length === 0) {\r\n          urlCreate = `${window.location.origin}/api/criarEvento`;\r\n        } else {\r\n          urlCreate = `${window.location.origin}/api/updateEvento/${ids}`;\r\n        }\r\n\r\n        await fetch(urlCreate, {\r\n          method: 'POST',\r\n          headers: { 'Content-Type': 'application/json' },\r\n          body: JSON.stringify(body),\r\n        });\r\n        setLoading(false);\r\n        setEditar(false);\r\n        mutate(url);\r\n      } catch (errors) {\r\n        console.errors();\r\n      }\r\n    }\r\n  };\r\n  function TelaEventos() {\r\n    return (\r\n      <Modal\r\n        open={open}\r\n        onClose={handleClose}\r\n        aria-labelledby=\"simple-modal-title\"\r\n        aria-describedby=\"simple-modal-description\"\r\n      >\r\n        <Box className={classes.boxUp}>\r\n          <Box className={classes.boxInterno}>\r\n            <MyDropzone />\r\n            {fileObjects.length > 4 && handleClick()}\r\n          </Box>\r\n        </Box>\r\n      </Modal>\r\n    );\r\n  }\r\n\r\n  // smDown>\r\n  return (\r\n    <>\r\n      {session ? (\r\n        <Box\r\n          mt={1}\r\n          className={classes.box}\r\n          width=\"100%\"\r\n          //            maxWidth={1200}\r\n          height=\"auto\"\r\n          borderRadius={16}\r\n        >\r\n          <form\r\n            noValidate\r\n            autoComplete=\"off\"\r\n            width=\"100%\"\r\n            className={classes.root}\r\n          >\r\n            <Box display=\"flex\" flexDirection=\"row\">\r\n              <Grid item xs={12} md={9}>\r\n                <Box className={classes.novoBox}>\r\n                  <TextField\r\n                    id=\"igreja\"\r\n                    label=\"Igreja\"\r\n                    variant=\"outlined\"\r\n                    v