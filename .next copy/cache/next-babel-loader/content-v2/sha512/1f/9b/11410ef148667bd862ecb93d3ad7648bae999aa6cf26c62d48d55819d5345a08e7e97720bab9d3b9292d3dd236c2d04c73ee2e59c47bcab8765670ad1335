      .then(() => {\r\n        // This will not throw error anymore\r\n        s3.getObject().createReadStream();\r\n        console.log('lugar 2- certo');\r\n      })\r\n      .catch((error) => {\r\n        console.log('lugar 3- erro');\r\n        if (error.statusCode === 404) {\r\n          // Catching NoSuchKey\r\n        }\r\n      });\r\n  };\r\n\r\n  handleDownload();\r\n};\r\n\r\nexport default downloadImg;\r\n"]},"metadata":{},"sourceType":"module"}