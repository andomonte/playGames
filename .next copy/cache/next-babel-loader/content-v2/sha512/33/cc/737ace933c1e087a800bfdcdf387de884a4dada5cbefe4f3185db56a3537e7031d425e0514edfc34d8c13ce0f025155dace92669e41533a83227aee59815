ionada[3] ? igrejaSelecionada[3].conversoes : '--',\r\n        igrejaSelecionada[4] ? igrejaSelecionada[4].conversoes : '--',\r\n        CabeçalhoTabela[4].media && CabeçalhoTabela[4].media,\r\n      ),\r\n    ];\r\n  }\r\n\r\n  if (data) {\r\n    dataIgreja = data;\r\n    for (let i = 0; i < dataIgreja.length; i += 1) {\r\n      newDadosRel[i] = dadosRel.filter(\r\n        (val) => val.codigoIgreja === data[i].codigoIgreja,\r\n      );\r\n    }\r\n    Panel2.push(\r\n      <FormControl\r\n        variant=\"outlined\"\r\n        className={classes.selectEmpty}\r\n        key=\"select\"\r\n        size=\"small\"\r\n      >\r\n        <FormHelperText>Escolha a Igreja</FormHelperText>\r\n\r\n        <Select\r\n          labelId=\"demo-simple-select-placeholder-label-label\"\r\n          id=\"demo-simple-select-placeholder-label\"\r\n          value={age}\r\n          onChange={handleSelect}\r\n          displayEmpty\r\n          className={classes.selectEmpty}\r\n        >\r\n          <MenuItem value={age}>\r\n            <em>Todas</em>\r\n          </MenuItem>\r\n\r\n          {dataIgreja?.map((items) => (\r\n            <MenuItem key={items.igreja} value={items.codigoIgreja}>\r\n              {items.igreja ?? items.igreja}\r\n            </MenuItem>\r\n          ))}\r\n        </Select>\r\n      </FormControl>,\r\n    );\r\n\r\n    if (igrejaSelecionada.length > 0) {\r\n      Tabelas.push(\r\n        <div key={igrejaSelecionada[1].id}>\r\n          <StyledTableContainer\r\n            component={Paper}\r\n            style={{ border: '1px solid rgba(0,0,0,0.2)', padding: 4 }}\r\n          >\r\n            <Table fixedHeader={false} style={{ tableLayout: 'auto' }}>\r\n              <TableHead>\r\n                <TableRow key={newDadosRel[1].id}>\r\n                  <TableCell\r\n                    align=\"center\"\r\n                    className={classes.tableCell}\r\n                    style={{ width: 50 }}\r\n                  >\r\n                    Semana\r\n                  </TableCell>\r\n                  <TableCell\r\n                    className={classes.tableCell}\r\n                    style={{ width: 30 }}\r\n                    align=\"right\"\r\n                  >\r\n                    1\r\n                  </TableCell>\r\n                  <TableCell\r\n                    className={classes.tableCell}\r\n                    style={{ width: 30 }}\r\n                    align=\"right\"\r\n                  >\r\n                    2\r\n                  </TableCell>\r\n                  <TableCell\r\n                    className={classes.tableCell}\r\n                    style={{ width: 20 }}\r\n                    align=\"right\"\r\n                  >\r\n                    Carbs\r\n                  </TableCell>\r\n                  <TableCell\r\n                    className={classes.tableCell}\r\n                    style={{ width: '25%' }}\r\n                    align=\"right\"\r\n                  >\r\n                    Protein\r\n                  </TableCell>\r\n                </TableRow>\r\n              </TableHead>\r\n              <TableBody>\r\n                {rows.map((row) => (\r\n                  <TableRow key={row.descricao}>\r\n                    <StyledTableCell component=\"th\" scope=\"row\">\r\n                      <Button\r\n                        variant=\"outlined\"\r\n                        style={{\r\n                          width: '100%',\r\n                          backgroundColor: 'rgba(0,0,0,0.1)',\r\n                        }}\r\n                      >\r\n                        {row.descricao}\r\n                      </Button>\r\n                    </StyledTableCell>\r\n                    <StyledTableCell align=\"right\">{row.sem1}</StyledTableCell>\r\n                    <StyledTableCell align=\"right\">{row.sem2}</StyledTableCell>\r\n                    <StyledTableCell align=\"right\">{row.sem3}</StyledTableCell>\r\n                    <StyledTableCell align=\"right\">{row.sem4}</StyledTableCell>\r\n                  </TableRow>\r\n                ))}\r\n              </TableBody>\r\n            </Table>\r\n          </StyledTableContainer>\r\n          ,\r\n        </div>,\r\n      );\r\n    }\r\n    if (igrejaSelecionada.length > 0) {\r\n      ListaIgrejas.push(\r\n        <div key={igrejaSelecionada[1].id}>\r\n          <TableContainer component={Paper}>\r\n            <Table\r\n              className={classes.table}\r\n              size=\"small\"\r\n              aria-label=\"a dense table\"\r\n            >\r\n              <TableHead size=\"small\">\r\n                <TableRow key={newDadosRel[1].id} size=\"small\">\r\n                  <TableCell align=\"center\" size=\"small\">\r\n                    Sem\r\n                  </TableCell>\r\n                  <TableCell size=\"small\" align=\"center\">\r\n                    1\r\n                  </TableCell>\r\n                  <TableCell size=\"small\" align=\"center\">\r\n                    2\r\n                  </TableCell>\r\n                  <TableCell size=\"small\" align=\"center\">\r\n                    3\r\n                  </TableCell>\r\n                  <TableCell size=\"small\" align=\"center\">\r\n                    4\r\n                  </TableCell>\r\n                  {qtSemana === 5 && (\r\n                    <TableCell size=\"small\" align=\"center\">\r\n                      5\r\n                    </TableCell>\r\n                  )}\r\n                  <TableCell size=\"small\" align=\"center\">\r\n                    M\r\n                  </TableCell>\r\n                </TableRow>\r\n              </TableHead>\r\n\r\n              <TableBody>\r\n                {rows.map((row) => (\r\n                  <TableRow key={row.descricao}>\r\n                    <TableCell component=\"th\" scope=\"row\">\r\n                      {row.descricao}\r\n                    </TableCell>\r\n                    <TableCell compone