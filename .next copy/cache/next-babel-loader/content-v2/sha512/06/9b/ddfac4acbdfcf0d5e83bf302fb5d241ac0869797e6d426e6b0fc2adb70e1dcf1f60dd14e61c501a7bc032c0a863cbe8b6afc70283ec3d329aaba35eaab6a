\r\n    );\r\n  }\r\n  console.log(dataNiver, mes);\r\n  const url = `${window.location.origin}/api/consultaRegiao/${item[0].RegiaoIDPB}/${mes}/${ano}`;\r\n  const { data, error } = useSWR(url, fetcher);\r\n  if (error)\r\n    return (\r\n      <div>\r\n        <MesageErro />\r\n      </div>\r\n    );\r\n  if (!data)\r\n    return (\r\n      <div>\r\n        <Loading />\r\n      </div>\r\n    );\r\n  //---------------------------------------------------------------------------\r\n\r\n  return (\r\n    <>\r\n      {session ? (\r\n        <Box\r\n          mt={3}\r\n          className={classes.box}\r\n          width=\"100%\"\r\n          //            maxWidth={1200}\r\n          height=\"auto\"\r\n        >\r\n          <Hidden smDown>\r\n            <Box width=\"100%\">\r\n              <Box className={classes.novoBox}>\r\n                <EventoDesk\r\n                  dadosRel={data}\r\n                  item={item}\r\n                  mes={mes}\r\n                  statusDrawer={statusDrawer}\r\n                />\r\n              </Box>\r\n            </Box>\r\n          </Hidden>\r\n          <Hidden mdUp>\r\n            <form\r\n              noValidate\r\n              autoComplete=\"off\"\r\n              width=\"100%\"\r\n              className={classes.root}\r\n            >\r\n              <Grid item xs={12} />\r\n              <Box display=\"flex\" flexDirection=\"row\">\r\n                <Grid item xs={12}>\r\n                  <Box className={classes.novoBox}>\r\n                    <EventoMobile\r\n                      dadosRel={data}\r\n                      item={item}\r\n                      mes={mes}\r\n                      statusDrawer={statusDrawer}\r\n                    />\r\n                  </Box>\r\n                </Grid>\r\n              </Box>\r\n            </form>\r\n          </Hidden>\r\n        </Box>\r\n      ) : (\r\n        signOut({\r\n          callbackUrl: `${window.location.origin}`,\r\n        })\r\n      )}\r\n    </>\r\n  );\r\n}\r\n\r\nexport default MostrarBusca;\r\n"]},"metadata":{},"sourceType":"module"}