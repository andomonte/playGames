------\r\n\r\n  if (data) {\r\n    showIgrejas.push(\r\n      <Box key={data}>\r\n        {data?.map((items) => (\r\n          <Box key={items.igreja} type=\"button\" justifyContent=\"flex-start\">\r\n            <Box display=\"flex\">\r\n              <Box mr={-2} ml={2} mt={2.2}>\r\n                <Avatar\r\n                  onClick={() => {\r\n                    handleFotos(items);\r\n                  }}\r\n                  alt=\"User\"\r\n                  className={classes.avatar}\r\n                  src={items.img01 ?? items.img01}\r\n                />\r\n                <Box\r\n                  onClick={() => {\r\n                    handleFotos(items);\r\n                  }}\r\n                  color=\"blue\"\r\n                  mt={2}\r\n                >\r\n                  ver fotos\r\n                </Box>\r\n              </Box>\r\n\r\n              <Box m={1} ml={5}>\r\n                <Typography\r\n                  variant=\"body1\"\r\n                  display=\"block\"\r\n                  gutterBottom\r\n                  align=\"left\"\r\n                  className={classes.caption}\r\n                >\r\n                  {items.igreja ?? items.igreja}\r\n                </Typography>\r\n                <Typography\r\n                  display=\"block\"\r\n                  variant=\"body2\"\r\n                  color=\"textSecondary\"\r\n                  align=\"left\"\r\n                  //                  style={{ marginRight: 60 }}\r\n                >\r\n                  <strong>Evento: </strong>\r\n                  {items.evento ?? items.evento}\r\n                </Typography>\r\n                <Box display=\"flex\" flexDirection=\"row\">\r\n                  <Grid item xs={6}>\r\n                    <Typography\r\n                      display=\"block\"\r\n                      variant=\"body2\"\r\n                      color=\"textSecondary\"\r\n                      align=\"left\"\r\n                      //     