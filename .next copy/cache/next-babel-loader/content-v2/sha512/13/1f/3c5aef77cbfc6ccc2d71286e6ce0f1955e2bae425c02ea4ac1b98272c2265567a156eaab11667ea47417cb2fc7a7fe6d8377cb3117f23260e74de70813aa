\n      error: false,\r\n\r\n      url: null,\r\n    }));\r\n    const array1 = ['oi'];\r\n\r\n    if (uploadedFiles) {\r\n      setState((old) => [...old, ...uploadedFiles]);\r\n      array1.concat(uploadedFiles);\r\n      console.log('array ->', array1);\r\n    }\r\n    for (let i = 0; i < state.length; i += 1) {\r\n      console.log('process->', state[i], i, files);\r\n    }\r\n    /* files.forEach((file) => URL.revokeObjectURL(file.preview));\r\n    setState(files);\r\n    concatFiles = concatFiles.concat(files); */\r\n  };\r\n  const thumbs = files.map((file) => (\r\n    <div key={file.id}>\r\n      <img src={file.preview} alt={file.name} height={1} />\r\n    </div>\r\n  ));\r\n\r\n  // clean up\r\n  const DragMessage = (a, b) => {\r\n    if (!a) {\r\n      return <UploadMessage>Arraste arquivos aqui..</UploadMessage>;\r\n    }\r\n    if (b) {\r\n      return (\r\n        <UploadMessage type=\"error\">Arquivo n√£o suportado..</UploadMessage>\r\n      );\r\n    }\r\n    return (\r\n      <UploadMessage type=\"success\">Solte os arquivos aqui..</UploadMessage>\r\n    );\r\n  };\r\n  return (\r\n    <section onLoad={handleUpload}>\r\n      <DropContainer\r\n        {...getRootProps()}\r\n        isDragActive={isDragActive}\r\n        isDragReject={isDragReject}\r\n      >\r\n        <input {...getInputProps()} />\r\n        {DragMessage(isDragActive, isDragReject)}\r\n      </DropContainer>\r\n\r\n      <aside>\r\n        {thumbs}\r\n        {state.length > 0 && <FileList files={state} />}\r\n      </aside>\r\n    </section>\r\n  );\r\n}\r\n\r\nexport default DropzoneComponent;\r\n"]},"metadata":{},"sourceType":"module"}