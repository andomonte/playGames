                       </TableRow>\r\n                      ))}\r\n                    </TableBody>\r\n                  </Table>\r\n                </TableContainer>\r\n              )}\r\n\r\n              <br />\r\n              <br />\r\n              {dadosRel.length === 0 ? (\r\n                <Box className={classes.box}>\r\n                  {!editar ? (\r\n                    <Button\r\n                      variant=\"contained\"\r\n                      color=\"primary\"\r\n                      size=\"small\"\r\n                      className={classes.button}\r\n                      startIcon={<AddIcon />}\r\n                      onClick={handleClick}\r\n                      mt={3}\r\n                      //  startIcon={<SaveIcon />}\r\n                    >\r\n                      Novo\r\n                    </Button>\r\n                  ) : (\r\n                    <Button\r\n                      variant=\"contained\"\r\n                      size=\"small\"\r\n                      color=\"secondary\"\r\n                      className={classes.buttonCancel}\r\n                      startIcon={<ReplyRoundedIcon />}\r\n                      onClick={handleClick}\r\n                      mt={3}\r\n                      //  startIcon={<SaveIcon />}\r\n                    >\r\n                      Voltar\r\n                    </Button>\r\n                  )}\r\n                  <Button\r\n                    variant=\"contained\"\r\n                    color=\"secondary\"\r\n                    size=\"small\"\r\n                    className={classes.button}\r\n                    startIcon={<SaveIcon />}\r\n                    onClick={submitData}\r\n                    mt={3}\r\n                    disabled={!!(!editar || !valid())}\r\n                    //  startIcon={<SaveIcon />}\r\n                  >\r\n                    Salvar\r\n                  </Button>\r\n                  {loading && (\r\n                    <CircularProgress\r