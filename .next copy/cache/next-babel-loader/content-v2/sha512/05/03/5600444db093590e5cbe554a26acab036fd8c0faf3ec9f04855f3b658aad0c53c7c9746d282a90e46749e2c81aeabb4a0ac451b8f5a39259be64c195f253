 }}\r\n                placeholder=\"\"\r\n                InputProps={{\r\n                  startAdornment: (\r\n                    <InputAdornment position=\"start\">R$</InputAdornment>\r\n                  ),\r\n                }}\r\n              />\r\n            )}\r\n          </Grid>\r\n        </Grid>\r\n        <br />\r\n        <br />\r\n        {dadosRel.length === 0 ? (\r\n          <Box className={classes.box}>\r\n            {!editar ? (\r\n              <Button\r\n                variant=\"contained\"\r\n                color=\"primary\"\r\n                size=\"small\"\r\n                className={classes.button}\r\n                startIcon={<AddIcon />}\r\n                onClick={handleClick}\r\n                mt={3}\r\n                //  startIcon={<SaveIcon />}\r\n              >\r\n                Novo\r\n              </Button>\r\n            ) : (\r\n              <Button\r\n                variant=\"contained\"\r\n                size=\"small\"\r\n                color=\"secundary\"\r\n                className={classes.buttonCancel}\r\n                startIcon={<ReplyRoundedIcon />}\r\n                onClick={handleClick}\r\n                mt={3}\r\n                //  startIcon={<SaveIcon />}\r\n              >\r\n                Voltar\r\n              </Button>\r\n            )}\r\n            {!editar || !valid() ? (\r\n              <Button\r\n                variant=\"contained\"\r\n                color=\"secondary\"\r\n                size=\"small\"\r\n                className={classes.button}\r\n                startIcon={<SaveIcon />}\r\n                onClick={submitData}\r\n                mt={3}\r\n                disabled\r\n                //  startIcon={<SaveIcon />}\r\n              >\r\n                Salvar\r\n              </Button>\r\n            ) : (\r\n              <Button\r\n                variant=\"contained\"\r\n                color=\"secondary\"\r\n                size=\"small\"\r\n                className={classes.button}\r\n                startIcon={<SaveIcon />}\r\n                onClick={submitData}\r\n                mt={3}\r\n\r\n                //  startIcon={<SaveIcon />}\r\n              >\r\n                Salvar\r\n              </Button>\r\n            )}\r\n          </Box>\r\n        ) : (\r\n          <Box className={classes.box}>\r\n            {!editar ? (\r\n              <Button\r\n                variant=\"contained\"\r\n                color=\"primary\"\r\n                size=\"small\"\r\n                className={classes.button}\r\n                startIcon={<EditIcon />}\r\n                onClick={handleClick}\r\n                mt={3}\r\n                //  startIcon={<SaveIcon />}\r\n              >\r\n                Editar\r\n              </Button>\r\n            ) : (\r\n              <Button\r\n                variant=\"contained\"\r\n                size=\"small\"\r\n                color=\"secundary\"\r\n                className={classes.buttonCancel}\r\n                startIcon={<ReplyRoundedIcon />}\r\n                onClick={handleClick}\r\n                mt={3}\r\n                //  startIcon={<SaveIcon />}\r\n              >\r\n                Voltar\r\n              </Button>\r\n            )}\r\n            {!editar || !valid() ? (\r\n              <Button\r\n                variant=\"contained\"\r\n                color=\"secondary\"\r\n                size=\"small\"\r\n                className={classes.button}\r\n                startIcon={<SaveIcon />}\r\n                onClick={submitData}\r\n                mt={3}\r\n                disabled\r\n                //  startIcon={<SaveIcon />}\r\n              >\r\n                Salvar\r\n              </Button>\r\n            ) : (\r\n              <Button\r\n                variant=\"contained\"\r\n                color=\"secondary\"\r\n                size=\"small\"\r\n                className={classes.button}\r\n                startIcon={<SaveIcon />}\r\n                onClick={submitData}\r\n                mt={3}\r\n\r\n                //  startIcon={<SaveIcon />}\r\n              >\r\n                Salvar\r\n              </Button>\r\n            )}\r\n          </Box>\r\n        )}\r\n      </form>\r\n    </Box>\r\n  );\r\n}\r\n\r\nexport default formulario;\r\n"]},"metadata":{},"sourceType":"module"}