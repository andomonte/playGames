        //  startIcon={<SaveIcon />}\r\n                    >\r\n                      Salvar\r\n                    </Button>\r\n                  )}\r\n                  {loading && (\r\n                    <CircularProgress\r\n                      size={24}\r\n                      className={classes.buttonProgress}\r\n                    />\r\n                  )}\r\n                </Box>\r\n              ) : (\r\n                <Box className={classes.box}>\r\n                  {!editar ? (\r\n                    <Button\r\n                      variant=\"contained\"\r\n                      color=\"primary\"\r\n                      size=\"small\"\r\n                      className={classes.button}\r\n                      startIcon={<EditIcon />}\r\n                      onClick={handleClick}\r\n                      mt={3}\r\n                      //  startIcon={<SaveIcon />}\r\n                    >\r\n                      Editar\r\n                    </Button>\r\n                  ) : (\r\n                    <Button\r\n                      variant=\"contained\"\r\n                      size=\"small\"\r\n                      color=\"secondary\"\r\n                      className={classes.buttonCancel}\r\n                      startIcon={<ReplyRoundedIcon />}\r\n                      onClick={handleClick}\r\n                      mt={3}\r\n                      //  startIcon={<SaveIcon />}\r\n                    >\r\n                      Voltar\r\n                    </Button>\r\n                  )}\r\n                  {!editar || !valid() ? (\r\n                    <Button\r\n                      variant=\"contained\"\r\n                      color=\"secondary\"\r\n                      size=\"small\"\r\n                      className={classes.button}\r\n                      startIcon={<SaveIcon />}\r\n                      onClick={submitData}\r\n                      mt={3}\r\n                      disabled\r\n                      //  startIcon={<SaveIcon />}\r\n                    >\r\n                      Salvar\r\n                    </Button>\r\n                  ) : (\r\n                    <Button\r\n                      variant=\"contained\"\r\n                      color=\"secondary\"\r\n                      size=\"small\"\r\n                      className={classes.button}\r\n                      startIcon={<SaveIcon />}\r\n                      onClick={submitData}\r\n                      mt={3}\r\n\r\n                      //  startIcon={<SaveIcon />}\r\n                    >\r\n                      Salvar\r\n                    </Button>\r\n                  )}\r\n                </Box>\r\n              )}\r\n              {loading && (\r\n                <CircularProgress\r\n                  size={24}\r\n                  className={classes.buttonProgress}\r\n                />\r\n              )}\r\n            </form>\r\n          </Hidden>\r\n          <Hidden mdUp>\r\n            <form\r\n              noValidate\r\n              autoComplete=\"off\"\r\n              width=\"100%\"\r\n              className={classes.root}\r\n            >\r\n              <Grid item xs={12}>\r\n                <Box className={classes.novoBox}>\r\n                  <TextField\r\n                    id=\"igreja\"\r\n                    label=\"Igreja\"\r\n                    variant=\"outlined\"\r\n                    value={item[0].igreja}\r\n                    disabled\r\n                    size=\"small\"\r\n                    className={classes.tf_m}\r\n                  />\r\n                </Box>\r\n              </Grid>\r\n              <Box flexDirection=\"row\" display={{ xs: 'none', sm: 'block' }}>\r\n                <Grid item xs={12}>\r\n                  <Box className={classes.novoBox}>\r\n                    <TextField\r\n                      id=\"codigoIgreja\"\r\n                      label=\"Código\"\r\n                      variant=\"outlined\"\r\n                      value={item[0].codigoIgreja}\r\n                      disabled\r\n                      className={classes.tf_m}\r\n                      size=\"small\"\r\n                    />\r\n                  </Box>\r\n                </Grid>\r\n              </Box>\r\n              <Box display=\"flex\" flexDirection=\"row\">\r\n                <Grid item xs={7}>\r\n                  <Box className={classes.novoBox}>\r\n                    {dadosRel.length !== 0 ? (\r\n                      <TextField\r\n                        id=\"data\"\r\n                        label=\"Data\"\r\n                        variant=\"outlined\"\r\n                        value={DataRelatorio}\r\n                        disabled\r\n                        className={classes.tf_m}\r\n                        size=\"small\"\r\n                      />\r\n                    ) : (\r\n                      <TextField\r\n                        id=\"data\"\r\n                        label=\"Data\"\r\n                        variant=\"outlined\"\r\n                        value={dataRelatorio}\r\n                        disabled\r\n                        className={classes.tf_m}\r\n                        size=\"small\"\r\n                        inputRef={atualizarData}\r\n                      />\r\n                    )}\r\n                  </Box>\r\n                </Grid>\r\n                <Grid item xs={5}>\r\n                  <Box className={classes.novoBox}>\r\n                    <TextField\r\n                      id=\"Semana\"\r\n                      label=\"Semana Nº:\"\r\n                      variant=\"outlined\"\r\n                      value={sem}\r\n                      disabled\r\n                      className={classes.tf_m}\r\n                      size=\"small\"\r\n                    />\r\n                  </Box>\r\n                </Grid>\r\n              </Box>\r\n\r\n              <br />\r\n              <Box display=\"flex\" flexDirection=\"row\">\r\n                <Grid item xs={6}>\r\n                  <Box className={classes.novoBox}>\r\n                    {editar ? (\r\n                      <TextField\r\n                        className={classes.tf_m}\r\n                        id=\"Adultos\"\r\n                        label=\"Adultos\"\r\n                        type=\"number\"\r\n                        InputLabelProps={{\r\n                          shrink: true,\r\n                        }}\r\n                        value={adultos}\r\n                        variant=\"outlined\"\r\n                        placeholder=\"\"\r\n                        size=\"small\"\r\n                        onBlur={\r\n                          adultos === ''\r\n                            ? () => setValidarAdultos('nao')\r\n                            : (() => setValidarAdultos('sim'),\r\n                              () => setContador('1'))\r\n                        }\r\n                        onChange={(e) => setAdultos(e.target.value)}\r\n                        error={validarCriancas === 'nao'}\r\n                        onFocus={(e) => setAdultos(e.target.value)}\r\n                        helperText={error ? 'Não pode ser Vazio!' : ''}\r\n                        inputRef={(input) => {\r\n                          if (input != null && contador === '0') {\r\n                            input.focus();\r\n                          }\r\n                        }}\r\n                      />\r\n                    ) : (\r\n                      <TextField\r\n                        className={classes.tf_m}\r\n                        id=\"Adultos\"\r\n                        label=\"Adultos\"\r\n                        type=\"number\"\r\n                        InputLabelProps={{\r\n                          shrink: true,\r\n                        }}\r\n                        value={Adultos}\r\n                        variant=\"outlined\"\r\n                        placeholder=\"\"\r\n                        size=\"small\"\r\n                        disabled\r\n                      />\r\n                    )}\r\n                  </Box>\r\n                </Grid>\r\n\r\n                <Grid item xs={6}>\r\n                  <Box className={classes.novoBox}>\r\n                    {editar ? (\r\n                      <TextField\r\n                        className={classes.tf_m}\r\n                        id=\"Crianças\"\r\n                        label=\"Crianças\"\r\n                        type=\"number\"\r\n                        InputLabelProps={{\r\n                          shrink: true,\r\n                        }}\r\n                        value={criancas}\r\n                        variant=\"outlined\"\r\n                        placeholder=\"\"\r\n                        size=\"small\"\r\n                        onBlur={\r\n                          criancas === ''\r\n                            ? () => setValidarCriancas('nao')\r\n                            : () => setValidarCriancas('sim')\r\n                        }\r\n                        onChange={(e) => setCriancas(e.target.value)}\r\n                        error={validarCriancas === 'nao'}\r\n                        onFocus={(e) => setCriancas(e.target.value)}\r\n                        helperText={error ? 'Não pode ser Vazio!' : ''}\r\n                      />\r\n                    ) : (\r\n                      <TextField\r\n                        className={classes.tf_m}\r\n                        id=\"Crianças\"\r\n                        label=\"Crianças\"\r\n                        type=\"number\"\r\n                        InputLabelProps={{\r\n                          shrink: true,\r\n                        }}\r\n                        value={Crianças}\r\n                        variant=\"outlined\"\r\n                        placeholder=\"\"\r\n                        size=\"small\"\r\n                        disabled\r\n                      />\r\n                    )}\r\n                  </Box