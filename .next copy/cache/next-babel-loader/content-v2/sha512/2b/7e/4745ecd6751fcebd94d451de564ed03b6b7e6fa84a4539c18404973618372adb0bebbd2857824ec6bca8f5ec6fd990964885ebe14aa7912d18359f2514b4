ad from 'next/head';\nimport { useState, useEffect } from 'react';\nimport { useRouter } from 'next/router';\nimport { signIn, useSession } from 'next-auth/client';\nimport styles from '../styles/pageStyles/login.module.scss';\n\nexport default function Login() {\n  const [email, setEmail] = useState('');\n  const [password, setPassword] = useState('');\n  const [isLoginStarted, setIsLoginStarted] = useState(false);\n  const [loginError, setLoginError] = useState('');\n  const router = useRouter();\n\n  useEffect(() => {\n    if (router.query.error) {\n      setLoginError(router.query.error);\n      setEmail(router.query.email);\n    }\n  }, [router]);\n\n  const handleLogin = (e) => {\n    console.log(email);\n    e.preventDefault();\n    setIsLoginStarted(true);\n    signIn('credentials', {\n      email,\n      password,\n      callbackUrl: `${window.location.origin}/welcome`,\n    });\n  };\n\n  return (\n    <div className={styles.container}>\n      <Head>\n        <title>NextAuth Example</title>\n      </Head>\n      <main className={styles.loginMain}>\n        <div className={styles.loginStep}>\n          <h1>SISTEMA IDPB</h1>\n          <form onSubmit={(e) => handleLogin(e)} className={styles.loginForm}>\n            <label htmlFor=\"loginEmail\">Email</label>\n            <input\n              id=\"loginEmail\"\n              type=\"email\"\n              value={email}\n              onChange={(e) => setEmail(e.target.value)}\n              className={loginError ? styles.errorInput : ''}\n            />\n            <span className={styles.error}>{loginError}</span>\n            <label htmlFor=\"inputPassword\">Password</label>\n            <input\n              id=\"inputPassword\"\n              type=\"password\"\n              value={password}\n              onChange={(e) => setPassword(e.target.value)}\n            />\n            <button\n              type=\"submit\"\n              disabled={isLoginStarted}\n              className={styles.blueButtonRo