rianças\"\r\n                    label=\"Crianças\"\r\n                    type=\"number\"\r\n                    InputLabelProps={{\r\n                      shrink: true,\r\n                    }}\r\n                    variant=\"outlined\"\r\n                    placeholder=\"\"\r\n                    size=\"small\"\r\n                    onBlur={\r\n                      criancas === ''\r\n                        ? () => setValidarCriancas('nao')\r\n                        : () => setValidarCriancas('sim')\r\n                    }\r\n                    onChange={(e) => setCriancas(e.target.value)}\r\n                    error={validarCriancas === 'nao'}\r\n                    onFocus={(e) => setCriancas(e.target.value)}\r\n                    helperText={error ? 'Não pode ser Vazio!' : ''}\r\n                    inputRef={\r\n                      !criancas && adultos\r\n                        ? (input) => input && input.focus()\r\n                        : null\r\n                    }\r\n                  />\r\n                ) : (\r\n                  <TextField\r\n                    className={classes.tf_m}\r\n                    id=\"Crianças\"\r\n                    label=\"Crianças\"\r\n                    type=\"number\"\r\n                    InputLabelProps={{\r\n                      shrink: true,\r\n                    }}\r\n                    value={Crianças}\r\n                    variant=\"outlined\"\r\n                    placeholder=\"\"\r\n                    size=\"small\"\r\n                    disabled\r\n                  />\r\n                )}\r\n              </Box>\r\n            </Grid>\r\n          </Box>\r\n\r\n          <Box display=\"flex\" flexDirection=\"row\">\r\n            <Grid item xs={6}>\r\n              <Box className={classes.novoBox}>\r\n                {editar ? (\r\n                  <TextField\r\n                    className={classes.tf_m}\r\n                    id=\"Visitantes\"\r\n                    label=\"