\r\n\r\n                    //  startIcon={<SaveIcon />}\r\n                  >\r\n                    Salvar\r\n                  </Button>\r\n                )}\r\n              </Box>\r\n            ) : (\r\n              <Box className={classes.box}>\r\n                {!editar ? (\r\n                  <Button\r\n                    variant=\"contained\"\r\n                    color=\"primary\"\r\n                    size=\"small\"\r\n                    className={classes.button}\r\n                    startIcon={<EditIcon />}\r\n                    onClick={handleClick}\r\n                    mt={3}\r\n                    //  startIcon={<SaveIcon />}\r\n                  >\r\n                    Editar\r\n                  </Button>\r\n                ) : (\r\n                  <Button\r\n                    variant=\"contained\"\r\n                    size=\"small\"\r\n                    color=\"secondary\"\r\n                    className={classes.buttonCancel}\r\n                    startIcon={<ReplyRoundedIcon />}\r\n                    onClick={handleClick}\r\n                    mt={3}\r\n                    //  startIcon={<SaveIcon />}\r\n                  >\r\n                    Voltar\r\n                  </Button>\r\n                )}\r\n                {!editar || !valid() ? (\r\n                  <Button\r\n                    variant=\"contained\"\r\n                    color=\"secondary\"\r\n                    size=\"small\"\r\n                    className={classes.button}\r\n                    startIcon={<SaveIcon />}\r\n                    onClick={submitData}\r\n                    mt={3}\r\n                    disabled\r\n                    //  startIcon={<SaveIcon />}\r\n                  >\r\n                    Salvar\r\n                  </Button>\r\n                ) : (\r\n                  <Button\r\n                    variant=\"contained\"\r\n                    color=\"secondary\"\r\n                    size=\"small\"\r\n                    className={classes.button}\r\n                    startIcon={<SaveIcon />}\r\n                    onClick={submitData}\r\n                    mt={3}\r\n\r\n                    //  startIcon={<SaveIcon />}\r\n                  >\r\n                    Salvar\r\n                  </Button>\r\n                )}\r\n                {loading && (\r\n                  <CircularProgress\r\n                    size={24}\r\n                    className={classes.buttonProgress}\r\n                  />\r\n                )}\r\n              </Box>\r\n            )}\r\n          </form>\r\n        </Box>\r\n      ) : (\r\n        signOut({\r\n          callbackUrl: `${window.location.origin}`,\r\n        })\r\n      )}\r\n    </>\r\n  );\r\n  return (\r\n    <Box>\r\n      <FormEvento />\r\n      {open && <TelaEventos />}\r\n    </Box>\r\n  );\r\n}\r\n\r\nexport default formulario;\r\n"]},"metadata":{},"sourceType":"module"}