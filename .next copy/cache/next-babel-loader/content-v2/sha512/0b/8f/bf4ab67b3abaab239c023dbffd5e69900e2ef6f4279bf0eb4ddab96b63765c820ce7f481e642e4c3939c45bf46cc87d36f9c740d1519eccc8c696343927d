pen) {\r\n      setOpen(false);\r\n    }\r\n  };\r\n  // document.documentElement.lang = 'pt-BR';\r\n  return (\r\n    <div onLoad={handleDrawerClose}>\r\n      <div className={classes.root}>\r\n        <AppBar className={classes.root2} color=\"default\">\r\n          <ClickAwayListener onClickAway={handleDrawerClose}>\r\n            <Toolbar className={classes.toolbar}>\r\n              <Box display=\"flex\" alignItems=\"center\">\r\n                {open ? (\r\n                  <MenuOpenIcon\r\n                    className={classes.hamburger}\r\n                    onClick={handleDrawerOpen}\r\n                  />\r\n                ) : null}\r\n                {!open ? (\r\n                  <MenuIcon\r\n                    className={classes.hamburger}\r\n                    onClick={handleDrawerOpen}\r\n                  />\r\n                ) : null}\r\n\r\n                <Hidden mdDown>\r\n                  <img\r\n                    src=\"/images/IDPBNAC.png\"\r\n                    alt=\"logo\"\r\n                    className={classes.logo}\r\n                  />\r\n                </Hidden>\r\n              </Box>\r\n\r\n              <Box display=\"flex\" m={0}>\r\n                <BottomNavigation\r\n                  value={value}\r\n                  onChange={(event, newValue) => {\r\n                    setValue(newValue);\r\n                  }}\r\n                  fontSize=\"large\"\r\n                  showLabels\r\n                  className={classes.rootTopbarIcon}\r\n                >\r\n                  <BottomNavigationAction\r\n                    label={LabelHome}\r\n                    icon={<PersonIcon />}\r\n                  />\r\n\r\n                  <BottomNavigationAction\r\n                    label={LabelIgreja}\r\n                    icon={<AccountBalanceIcon />}\r\n                  />\r\n                  <BottomNavigationAction\r\n                    label={LabelEquipe}\r\n                    icon={<GroupWorkIcon />}\r\n                  />\r\n                </BottomNavigation>\r\n              </Box>\r\n              <Login />\r\n            </Toolbar>\r\n          </ClickAwayListener>\r\n        </AppBar>\r\n\r\n        <Drawer\r\n          variant=\"persistent\"\r\n          anchor=\"left\"\r\n          open={open}\r\n          className={classes.drawer}\r\n          classes={{ paper: classes.desktopDrawer }}\r\n        >\r\n          {perfilUser === 'ministro' && (\r\n            <NavbarMinistro perfilUser={perfilUser} />\r\n          )}\r\n          {perfilUser === 'sup-MM' && <NavbarSupMM perfilUser={perfilUser} />}\r\n          {perfilUser === 'coord-MM' && (\r\n            <NavbarCoordMM perfilUser={perfilUser} />\r\n          )}\r\n          {perfilUser === 'dir-MM' && <NavbarCoordMM perfilUser={perfilUser} />}\r\n        </Drawer>\r\n\r\n        <main\r\n          className={clsx(classes.contentMain, {\r\n            [classes.contentShiftMain]: open,\r\n          })}\r\n        >\r\n          <div className={classes.drawerHeader} />\r\n          {/* {children} */}\r\n\r\n          <TabPanel value={value} index={0}>\r\n            {session && (\r\n              <Box>\r\n                {data.length ? (\r\n                  <MudarDados\r\n                    ministros={data}\r\n                    item={item}\r\n                    secao={session}\r\n                    perfilUser={perfilUser}\r\n                    statusDrawer={open}\r\n                  />\r\n                ) : (\r\n                  <Box\r\n                    height=\"90vh\"\r\n                    display=\"flex\"\r\n                    alignItems=\"center\"\r\n                    justifyContent=\"center\"\r\n                  >\r\n                    <Box>\r\n                      <Grid item xs={12}>\r\n                        <img src=\"/images/idpb.ico\" alt=\"\" width=\"125\" />\r\n                      </Grid>\r\n                    </Box>\r\n                    <Grid item xs={12}>\r\n                      <strong> Apenas para Ministros da IDPB </strong>\r\n                    </Grid>\r\n                  </Box>\r\n                )}\r\n              </Box>\r\n            )}\r\n          </TabPanel>\r\n          <TabPanel value={value} index={1}>\r\n            {session && (\r\n              <Igreja item={item} secao={session} igrejas={igrejas} />\r\n            )}\r\n          </TabPanel>\r\n          <TabPanel value={value} index={2}>\r\n            {perfilUser === 'ministro' ? <Padrao /> : null}\r\n            {perfilUser === 'sup-MM' ? <Padrao /> : null}\r\n            {perfilUser === 'coord-MM' ? <Padrao /> : null}\r\n            {perfilUser === 'dir-MM' ? <Padrao /> : null}\r\n            {perfilUser === 'adm_MM' ? <Padrao /> : null}\r\n          </TabPanel>\r\n        </main>\r\n      </div>\r\n    </div>\r\n  );\r\n}\r\n\r\nexport { PageAtualizar, TabPanel };\r\n"]},"metadata":{},"sourceType":"module"}