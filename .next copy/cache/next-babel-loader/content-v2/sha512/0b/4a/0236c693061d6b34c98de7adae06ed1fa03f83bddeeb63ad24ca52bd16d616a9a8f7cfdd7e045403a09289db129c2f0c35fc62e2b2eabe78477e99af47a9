\r\n    setOpen(true);\r\n  };\r\n\r\n  const handleClose = () => {\r\n    setOpen(false);\r\n  };\r\n  //--------------------------------------------------------------------------\r\n\r\n  //--------------------------------------------------------------------------\r\n\r\n  if (dadosUser) {\r\n    showIgrejas.push(\r\n      <Box key={dadosUser}>\r\n        {dadosUser?.map((items) => (\r\n          <Box key={items.id} type=\"button\" justifyContent=\"flex-start\">\r\n            <Box display=\"flex\">\r\n              <Box mr={-2} ml={2} mt={2.2}>\r\n                <Avatar\r\n                  onClick={() => {\r\n                    handleFotos(items);\r\n                  }}\r\n                  alt=\"User\"\r\n                  className={classes.avatar}\r\n                  src={items.img01 ?? items.img01}\r\n                />\r\n                <Box\r\n                  onClick={() => {\r\n                    handleFotos(items);\r\n                  }}\r\n                  color=\"blue\"\r\n                  mt={2}\r\n                >\r\n                  ver fotos\r\n                </Box>\r\n              </Box>\r\n\r\n              <Box m={1} ml={5}>\r\n                <Typography\r\n                  variant=\"body1\"\r\n                  display=\"block\"\r\n                  gutterBottom\r\n                  align=\"left\"\r\n                  className={classes.caption}\r\n                >\r\n                  {items.igreja ?? items.igreja}\r\n                </Typography>\r\n                <Typography\r\n                  display=\"block\"\r\n                  variant=\"body2\"\r\n                  color=\"textSecondary\"\r\n                  align=\"left\"\r\n                  //                  style={{ marginRight: 60 }}\r\n                >\r\n                  <strong>Evento: </strong>\r\n                  {items.evento ?? items.evento}\r\n                </Typography>\r\n                <Box display=\"flex\" flexDirection=\"row\">\r\n        