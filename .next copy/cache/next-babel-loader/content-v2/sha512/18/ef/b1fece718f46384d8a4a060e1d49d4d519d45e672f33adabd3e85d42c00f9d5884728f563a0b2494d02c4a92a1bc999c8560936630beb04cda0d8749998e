sim');\r\n  const [validarCriancas, setValidarCriancas] = React.useState('sim');\r\n  const [validarVisitantes, setValidarVisitantes] = React.useState('sim');\r\n  const [validarConversoes, setValidarConversoes] = React.useState('sim');\r\n  const [validarOfertas, setValidarOfertas] = React.useState('sim');\r\n  // const dia = Data.getDate().toString();\r\n  // console.log(Data.slice(3)); // retorna 'Rica'\r\n  //  console.log(Data, { sem });\r\n  //--------------------------------------------------------------------------\r\n\r\n  //--------------------------------------------------------------------------\r\n  const valid = () => {\r\n    console.log({ criancas });\r\n    if (!adultos || !criancas || !visitantes || !conversoes || !ofertas) {\r\n      return false;\r\n    }\r\n    return true;\r\n  };\r\n  //--------------------------------------------------------------------------\r\n  //--------------------------------------------------------------------------\r\n\r\n  const submitData = async (e) => {\r\n    e.preventDefault();\r\n    const valida = valid();\r\n    console.log('entrou no submit:', valida);\r\n    // const dataRelatorio = DataRelatorio;\r\n\r\n    if (valida) {\r\n      try {\r\n        const body = {\r\n          igreja,\r\n          codigoIgreja,\r\n          semana,\r\n          mes,\r\n          ano,\r\n          adultos,\r\n          criancas,\r\n          visitantes,\r\n          conversoes,\r\n          dataRelatorio,\r\n          ofertas,\r\n        };\r\n        console.log('dados enviados:', body);\r\n        let urlCreate = '';\r\n        if (dadosRel.length === 0) {\r\n          urlCreate = `${window.location.origin}/api/criarRelatorio`;\r\n        } else {\r\n          urlCreate = `${window.location.origin}/api/updateRelatorio`;\r\n        }\r\n        await fetch(urlCreate, {\r\n          method: 'POST',\r\n          headers: { 'Content-Type': 'application/json' },\r\n          body: JSON.stringify(body),\r\n        });\r\n        //    console.log('dados criados', fetch);\r\n        // await Router.push('/drafts');\r\n      } catch (errors) {\r\n        //     console.error(errors);\r\n      }\r\n    }\r\n  };\r\n  return (\r\n    <Box\r\n      mt={3}\r\n      className={classes.box}\r\n      width=\"100%\"\r\n      //            maxWidth={1200}\r\n      height=\"auto\"\r\n      borderRadius={16}\r\n    >\r\n      <form noValidate autoComplete=\"off\" width=\"100%\" className={classes.root}>\r\n        <Grid container spacing={3}>\r\n          <Grid item xs={12} md={12} lg={12} xl={12}>\r\n            <TextField\r\n              id=\"igreja\"\r\n              label=\"Igreja\"\r\n              variant=\"outlined\"\r\n              value={item[0].igreja}\r\n              disabled\r\n              size=\"small\"\r\n              className={classes.textField1}\r\n            />\r\n\r\n            <TextField\r\n              id=\"codigoIgreja\"\r\n              label=\"Código da Igreja\"\r\n              variant=\"outlined\"\r\n              value={item[0].codigoIgreja}\r\n              disabled\r\n              className={classes.textField2}\r\n              size=\"small\"\r\n            />\r\n            <TextField\r\n              id=\"data\"\r\n              label=\"Data do Relatório\"\r\n              variant=\"outlined\"\r\n              value={DataRelatorio}\r\n              disabled\r\n              className={classes.textField2}\r\n              size=\"small\"\r\n            />\r\n\r\n            <TextField\r\n              id=\"Semana\"\r\n              label=\"Semana Nº:\"\r\n              variant=\"outlined\"\r\n              value={sem}\r\n              disabled\r\n              className={classes.textField3}\r\n              size=\"small\"\r\n            />\r\n          </Grid>\r\n        </Grid>\r\n        <br />\r\n        <br />\r\n        <Grid container spacing={3}>\r\n          <Grid item xs={12} md={12} lg={12} xl={12}>\r\n            {editar ? (\r\n              <TextField\r\