   error: false,\r\n            url: null,\r\n          }),\r\n        ),\r\n      );\r\n      setFileObjects([].concat(fileObjects, acceptedFiles));\r\n    });\r\n\r\n    const {\r\n      getRootProps,\r\n      getInputProps,\r\n      isDragActive,\r\n      isDragReject,\r\n    } = useDropzone({\r\n      onDrop,\r\n\r\n      accept: 'image/*',\r\n    });\r\n\r\n    const fileSend = () => {\r\n      for (let i = 0; i < 5; i += 1) {\r\n        uploadedFiles[i] = fileObjects[i];\r\n      }\r\n      setOpen(true);\r\n    };\r\n\r\n    const DragMessage = (a, b) => {\r\n      if (!a) {\r\n        return (\r\n          <UploadMessage>\r\n            <Button>\r\n              <ImageSearchIcon fontSize=\"large\" style={{ color: 'black' }} />\r\n            </Button>\r\n            Pressione Aqui para Inseir Imagens\r\n          </UploadMessage>\r\n        );\r\n      }\r\n      if (b) {\r\n        return (\r\n          <UploadMessage type=\"error\">Arquivo não suportado..</UploadMessage>\r\n        );\r\n      }\r\n      return (\r\n        <UploadMessage type=\"success\">Solte os arquivos aqui..</UploadMessage>\r\n      );\r\n    };\r\n\r\n    return (\r\n      <>\r\n        <Dropzone>\r\n          {() => (\r\n            <DropContainer\r\n              {...getRootProps()}\r\n              isDragActive={isDragActive}\r\n              isDragReject={isDragReject}\r\n            >\r\n              <input {...getInputProps()} />\r\n              {DragMessage(isDragActive, isDragReject)}\r\n            </DropContainer>\r\n          )}\r\n        </Dropzone>\r\n\r\n        {fileObjects.length > 0 && <FileList files={fileObjects} />}\r\n        {fileObjects.length > 4 && fileSend()}\r\n        <Box display=\"flex\" flexDirection=\"row\" justifyContent=\"center\">\r\n          <Button\r\n            variant=\"contained\"\r\n            size=\"small\"\r\n            color=\"secondary\"\r\n            onClick={handleVoltar}\r\n          >\r\n            Voltar\r\n          </Button>\r\n        </Box>\r\n      </>\r\n    );\r\n  }\r\n\r\n  function TelaEventos() {\r\n    return (\r\n      <Modal\r\n        open={open}\r\n        onClose={handleClose}\r\n        aria-labelledby=\"simple-modal-title\"\r\n        aria-describedby=\"simple-modal-description\"\r\n      >\r\n        <Box className={classes.boxUp}>\r\n          <Box className={classes.boxInterno}>\r\n            <MyDropzone />\r\n          </Box>\r\n        </Box>\r\n      </Modal>\r\n    );\r\n  }\r\n\r\n  // const [editar, setEditar] = React.useState(true);\r\n\r\n  const { data, error } = useSWR(url, fetcher);\r\n  // useSWR('/api/user', (id = 4) => fetcher(id));\r\n  // useSWR('/api/consultaDados', fetcher);\r\n  if (error) return <div>Failed to load</div>;\r\n  if (!data) return <div>Loading...</div>;\r\n  //---------------------------------------------------------------------------\r\n  const dadosRel = data.filter((val) => val.semana === Semana);\r\n  let Evento = '';\r\n  let Adultos = '';\r\n  let Crianças = '';\r\n  let Visitantes = '';\r\n  let Conversoes = '';\r\n  let ids = '';\r\n  const Descriçao = '';\r\n\r\n  if (dadosRel.length !== 0) {\r\n    Evento = dadosRel[0].evento;\r\n    Adultos = dadosRel[0].adultos;\r\n    Crianças = dadosRel[0].criancas;\r\n    Visitantes = dadosRel[0].visitantes;\r\n    Conversoes = dadosRel[0].conversoes;\r\n    ids = dadosRel[0].id;\r\n  }\r\n\r\n  //--------------------------------------------------------------------------\r\n  //--------------------------------------------------------------------------\r\n  const valid = () => {\r\n    if (!adultos || !criancas || !visitantes || !conversoes || !evento) {\r\n      return false;\r\n    }\r\n    return true;\r\n  };\r\n  //--------------------------------------------------------------------------\r\n  const handleClick = () => {\r\n    setEvento(Evento);\r\n    setAdultos(Adultos);\r\n    setCriancas(Crianças);\r\n    setVisitantes(Visitantes);\r\n    setConversoes(Conversoes);\r\n    setDescriçao(Descriçao);\r\n\r\n    if (!editar) {\r\n      setEditar(true);\r\n      setContador('0');\r\n    } else {\r\n      setEditar(false);\r\n    }\r\n  };\r\n  //--------------------------------------------------------------------------\r\n  //--------------------------------------------------------------------------\r\n\r\n  const submitData = async (e) => {\r\n    e.preventDefault();\r\n    const valida = valid();\r\n    setLoading(true);\r\n\r\n    if (valida) {\r\n      try {\r\n        const body = {\r\n          evento,\r\n          igreja,\r\n          codigoIgreja,\r\n          dataEvento,\r\n          adultos,\r\n          criancas,\r\n          visitantes,\r\n          conversoes,\r\n          img01,\r\n          img02,\r\n          img03,\r\n          img04,\r\n          img05,\r\n        };\r\n\r\n        let urlCreate = '';\r\n        if (dadosRel.length === 0) {\r\n          urlCreate = `${window.location.origin}/api/criarEvento`;\r\n        } else {\r\n          urlCreate = `${window.location.origin}/api/updateEvento/${ids}`;\r\n        }\r\n\r\n        await fetch(urlCreate, {\r\n          method: 'POST',\r\n          headers: { 'Content-Type': 'application/json' },\r\n          body: JSON.stringify(body),\r\n        });\r\n        setLoading(false);\r\n        setEditar(false);\r\n        mutate(url);\r\n      } catch (errors) {\r\n        console.errors();\r\n      }\r\n    }\r\n  };\r\n\r\n  // smDown>\r\n  return (\r\n    <>\r\n      {session ? (\r\n        <Box\r\n          mt={1}\r\n          className={classes.box}\r\n          width=\"100%\"\r\n          //            maxWidth={1200}\r\n          height=\"auto\"\r\n          borderRadius={16}\r\n        >\r\n          <form\r\n            noValidate\r\n            autoComplete=\"off\"\r\n            width=\"100%\"\r\n            className={classes.root}\r\n          >\r\n            <Box display=\"flex\" flexDirection=\"row\">\r\n              <Grid item xs={12} md={9}>\r\n                <Box className={classes.novoBox}>\r\n                  <TextField\r\n                    id=\"igreja\"\r\n                    label=\"Igreja\"\r\n                    variant=\"outlined\"\r\n                    value={item[0].igreja}\r\n                    disabled\r\n                    size=\"small\"\r\n                    className={classes.tf_m}\r\n                  />\r\n                </Box>\r\n              </Grid>\r\n              <Hidden smDown>\r\n                <Grid item xs={3}>\r\n                  <Box className={classes.novoBox}>\r\n                    <TextField\r\n                      id=\"codigoIgreja\"\r\n                      label=\"Código\"\r\n                      variant=\"outlined\"\r\n                      value={item[0].codigoIgreja}\r\n                      disabled\r\n                      className={classes.tf_m}\r\n                      size=\"small\"\r\n                    />\r\n                  </Box>\r\n                </Grid>\r\n              </Hidden>\r\n            </Box>\r\n            <br />\r\n            <Box display=\"flex\" flexDirection=\"row\">\r\n              <Grid item xs={12}>\r\n                <Box className={classes.novoBox}>\r\n                  {editar ? (\r\n                    <TextField\r\n                      className={classes.tf_m}\r\n                      id=\"Evento\"\r\n                      label=\"Evento\"\r\n                      type=\"string\"\r\n                      InputLabelProps={{\r\n                        shrink: true,\r\n                      }}\r\n                      value={evento}\r\n                      variant=\"outlined\"\r\n                      placeholder=\"Qual foi o Evento\"\r\n                      size=\"small\"\r\n                      onBlur={\r\n                        evento === ''\r\n                          ? () => setValidarEvento('nao')\r\n                          : (() => setValidarEvento('sim'),\r\n                            () => setContador('1'))\r\n                      }\r\n                      onChange={(e) => setEvento(e.target.value)}\r\n                      error={validarEvento === 'nao'}\r\n                      onFocus={(e) => setEvento(e.target.value)}\r\n                      helperText={error ? 'Não pode ser Vazio!' : ''}\r\n                      inputRef={(input) => {\r\n                        if (input != null && contador === '0') {\r\n                          input.focus();\r\n                        }\r\n                      }}\r\n                    />\r\n                  ) : (\r\n                    <TextField\r\n                      className={classes.tf_m}\r\n                      id=\"Evento\"\r\n                      label=\"Evento\"\r\n                      type=\"string\"\r\n                      InputLabelProps={{\r\n                        shrink: true,\r\n                      }}\r\n                      value={Evento}\r\n                      variant=\"outlined\"\r\n                      placeholder=\"Qual foi o Evento\"\r\n                      size=\"small\"\r\n                      disabled\r\n                    />\r\n                  )}\r\n                </Box>\r\n              </Grid>\r\n            </Box>\r\n            <Box display=\"flex\" flexDirection=\"row\">\r\n              <Grid item xs={6}>\r\n                <Box className={classes.novoBox}>\r\n                  {editar ? (\r\n                    <TextField\r\n                      className={classes.tf_m}\r\n                      id=\"Adultos\"\r\n                      label=\"Adultos\"\r\n                      type=\"number\"\r\n                      InputLabelProps={{\r\n                        shrink: true,\r\n                      }}\r\n                      value={adultos}\r\n                      variant=\"outlined\"\r\n                      placeholder=\"\"\r\n                      size=\"small\"\r\n                      onBlur={\r\n                        adultos === ''\r\n                          ? (