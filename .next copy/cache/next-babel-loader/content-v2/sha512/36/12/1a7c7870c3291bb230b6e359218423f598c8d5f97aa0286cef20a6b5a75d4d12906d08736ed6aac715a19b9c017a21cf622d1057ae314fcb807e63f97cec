acceptedFiles));\r\n      setContagem([].concat(contagem, acceptedFiles));\r\n    });\r\n\r\n    const {\r\n      getRootProps,\r\n      getInputProps,\r\n      isDragActive,\r\n      isDragReject,\r\n    } = useDropzone({\r\n      onDrop,\r\n\r\n      accept: 'image/*',\r\n    });\r\n\r\n    const DragMessage = (a, b) => {\r\n      if (send) {\r\n        return (\r\n          <UploadMessage>\r\n            <CheckCircleIcon style={{ color: '#76ff03', fontSize: 50 }} />\r\n          </UploadMessage>\r\n        );\r\n      }\r\n      if (!a && !send) {\r\n        return (\r\n          <UploadMessage>\r\n            <Button>\r\n              <ImageSearchIcon fontSize=\"large\" style={{ color: 'black' }} />\r\n            </Button>\r\n            Pressione Aqui para Inseir o Vídeo\r\n          </UploadMessage>\r\n        );\r\n      }\r\n      if (b && !send) {\r\n        return (\r\n          <UploadMessage type=\"error\">Arquivo não suportado..</UploadMessage>\r\n        );\r\n      }\r\n      return (\r\n        <UploadMessage type=\"success\">Solte os arquivos aqui..</UploadMessage>\r\n      );\r\n    };\r\n\r\n    return (\r\n      <>\r\n        <Dropzone>\r\n          {() => (\r\n            <DropContainer\r\n              {...getRootProps({\r\n                onClick: (event) => {\r\n                  if (send) {\r\n                    event.stopPropagation();\r\n                  }\r\n                },\r\n              })}\r\n              isDragActive={isDragActive}\r\n              isDragReject={isDragReject}\r\n            >\r\n              <input {...getInputProps()} />\r\n              {DragMessage(isDragActive, isDragReject)}\r\n            </DropContainer>\r\n          )}\r\n        </Dropzone>\r\n        {/* type: file.name.str.substr('.'), */}\r\n        {fileObjects.length > 0 && <FileList files={fileObjects} />}\r\n      </>\r\n    );\r\n  }\r\n\r\n  // const [editar, setEditar] = React.useState(true);\r\n\r\n  const { data, error } = useSWR(url, fetcher);\r\n  // useSWR('/api/user', (id = 4) => fetcher(id));\r\n  // useSWR('/api/consultaDados', fetcher);\r\n  if (error) return <div>Failed to load</div>;\r\n  if (!data) return <div>Loading...</div>;\r\n  //---------------------------------------------------------------------------\r\n  //--------------------------------------------------------------------------\r\n  const handleModal = () => {\r\n    setOpen(true);\r\n    setSend(false);\r\n  };\r\n\r\n  const handleContImage = () => {\r\n    setContImage(contImage + 1);\r\n    if (contImage > 3) setContImage(0);\r\n  };\r\n  //--------------------------------------------------------------------------\r\n  //--------------------------------------------------------------------------\r\n\r\n  function ShowImage() {\r\n    arrayImage = [Img01, Img02, Img03, Img04, Img05];\r\n\r\n    return (\r\n      <Modal\r\n        open={openImage}\r\n        onClose={handleCloseImagem}\r\n        aria-labelledby=\"simple-modal-title\"\r\n        aria-describedby=\"simple-modal-description\"\r\n      >\r\n        <Box className={classes.boxUp}>\r\n          <ClickAwayListener onClickAway={handleCloseImagem}>\r\n            <Box className={classes.boxImage}>\r\n              <Image onClick={handleContImage} src={arrayImage[contImage]} />\r\n              <Box\r\n                display=\"flex\"\r\n                flexDirection=\"row\"\r\n                justifyContent=\"space-around\"\r\n                mt={2}\r\n              >\r\n                <Button\r\n                  variant=\"contained\"\r\n                  size=\"small\"\r\n                  onClick={handleCloseImagem}\r\n                  className={classes.buttonCancel}\r\n                >\r\n                  Fechar\r\n                </Button>\r\n                <Button\r\n                  variant=\"contained\"\r\n                  size=\"small\"\r\n                  className={classes.buttonGreen}\r\n                  onClick={handleContImage}\r\n                >\r\n                  Próxima\r\n                </Button>\r\n              </Box>\r\n            </Box>\r\n          </ClickAwayListener>\r\n        </Box>\r\n      </Modal>\r\n    );\r\n  }\r\n  function TelaEventos() {\r\n    return (\r\n      <Modal\r\n        open={open}\r\n        onClose={handleClose}\r\n        aria-labelledby=\"simple-modal-title\"\r\n        aria-describedby=\"simple-modal-description\"\r\n      >\r\n        <Box className={classes.boxUp}>\r\n          <Box className={classes.boxInterno}>\r\n            <MyDropzone />\r\n\r\n            <Box\r\n              display=\"flex\"\r\n              flexDirection=\"row\"\r\n              justifyContent=\"space-around\"\r\n              mt={2}\r\n            >\r\n              <Button\r\n                variant=\"contained\"\r\n                size=\"small\"\r\n                onClick={handleVoltar}\r\n                className={classes.buttonCancel}\r\n              >\r\n                Cancelar\r\n              </Button>\r\n              <Button\r\n                variant=\"contained\"\r\n                size=\"small\"\r\n                className={classes.buttonGreen}\r\n                onClick={fileSend}\r\n                disabled={!send}\r\n              >\r\n                Adicionar\r\n              </Button>\r\n            </Box>\r\n          </Box>\r\n        </Box>\r\n      </Modal>\r\n    );\r\n  }\r\n\r\n  return (\r\n    <>\r\n      {session ? (\r\n        <Box\r\n          mt={1}\r\n          className={classes.box}\r\n          width=\"100%\"\r\n          //            maxWidth={1200}\r\n          height=\"auto\"\r\n          borderRadius={16}\r\n        >\r\n          <form\r\n            noValidate\r\n            autoComplete=\"off\"\r\n            width=\"100%\"\r\n            className={classes.root}\r\n          >\r\n            <Box display=\"flex\" flexDirection=\"row\">\r\n              <Grid item xs={12} md={9}>\r\n                <Box className={classes.novoBox}>\r\n                  <TextField\r\n                    id=\"igreja\"\r\n                    label=\"Igreja\"\r\n                    variant=\"outlined\"\r\n                    value={item[0].igreja}\r\n                    disabled\r\n                    size=\"small\"\r\n                    className={classes.tf_m}\r\n                  />\r\n                </Box>\r\n              </Grid>\r\n              <Hidden smDown>\r\n                <Grid item xs={3}>\r\n                  <Box className={classes.novoBox}>\r\n                    <TextField\r\n                      id=\"codigoIgreja\"\r\n                      label=\"Código\"\r\n                      variant=\"outlined\"\r\n                      value={item[0].codigoIgreja}\r\n                      disabled\r\n                      className={classes.tf_m}\r\n                      size=\"small\"\r\n                    />\r\n                  </Box>\r\n                </Grid>\r\n              </Hidden>\r\n            </Box>\r\n            <br />\r\n\r\n            <Box className={classes.box}>\r\n              <Button\r\n                variant=\"contained\"\r\n                color=\"primary\"\r\n                size=\"small\"\r\n                className={classes.button}\r\n                startIcon={<AddIcon />}\r\n                onClick={handleModal}\r\n                mt={2}\r\n                disabled={loading}\r\n                //  startIcon={<SaveIcon />}\r\n              >\r\n                Novo\r\n              </Button>\r\n\r\n              {loading && (\r\n                <CircularProgress\r\n                  size={40}\r\n                  className={classes.buttonProgress}\r\n                  value={progresso}\r\n                />\r\n              )}\r\n            </Box>\r\n          </form>\r\n        </Box>\r\n      ) : (\r\n        signOut({\r\n          callbackUrl: `${window.location.origin}`,\r\n        })\r\n      )}\r\n      {open && <TelaEventos />}\r\n      {openImage && <ShowImage />}\r\n    </>\r\n  );\r\n}\r\n\r\nexport default enviar;\r\n"]},"metadata":{},"sourceType":"module"}