 </TableCell>\r\n            </TableRow>\r\n          </TableHead>\r\n          <TableBody>\r\n            {rows.map((row) => (\r\n              <TableRow\r\n                key={row.descricao}\r\n                style={{ width: larg, backgroundColor: '#ffff8d' }}\r\n                height={30}\r\n              >\r\n                <StyledTableCell align=\"center\" component=\"th\" scope=\"row\">\r\n                  <Box\r\n                    style={{\r\n                      width: largRotulo,\r\n                      fontSize: '10px',\r\n                    }}\r\n                  >\r\n                    {row.descricao}\r\n                  </Box>\r\n                </StyledTableCell>\r\n                <StyledTableCell\r\n                  style={{ width: larg, backgroundColor: '#448aff' }}\r\n                  align=\"center\"\r\n                >\r\n                  <Box\r\n                    style={{\r\n                      width: larg,\r\n                      fontSize: '12px',\r\n                      height: '100%',\r\n                    }}\r\n                  >\r\n                    {row.sem1}\r\n                  </Box>\r\n                </StyledTableCell>\r\n                <StyledTableCell\r\n                  align=\"center\"\r\n                  style={{ backgroundColor: '#ffff8d' }}\r\n                >\r\n                  <Box\r\n                    variant=\"outlined\"\r\n                    style={{\r\n                      width: larg,\r\n                      fontSize: '12px',\r\n                    }}\r\n                  >\r\n                    {row.sem2}\r\n                  </Box>\r\n                </StyledTableCell>\r\n                <StyledTableCell\r\n                  align=\"center\"\r\n                  style={{ backgroundColor: '#448aff' }}\r\n                >\r\n                  <Box\r\n                    variant=\"outlined\"\r\n                    style={{\r\n                      width: larg,\r\n                      fontSize: '12px',\r\n                    }}\r\n                  >\r\n                    {row.sem3}\r\n                  </Box>\r\n                </StyledTableCell>\r\n                <StyledTableCell\r\n                  align=\"center\"\r\n                  style={{ backgroundColor: '#ffff8d' }}\r\n                >\r\n                  <Box\r\n                    variant=\"outlined\"\r\n                    style={{\r\n                      width: larg,\r\n                      fontSize: '12px',\r\n                    }}\r\n                  >\r\n                    {row.sem4}\r\n                  </Box>\r\n                </StyledTableCell>\r\n                <StyledTableCell\r\n                  align=\"center\"\r\n                  style={{ backgroundColor: '#448aff' }}\r\n                >\r\n                  <Box\r\n                    variant=\"outlined\"\r\n                    style={{\r\n                      width: larg,\r\n                      fontSize: '12px',\r\n                    }}\r\n                  >\r\n                    {row.sem5}\r\n                  </Box>\r\n                </StyledTableCell>\r\n                <StyledTableCell\r\n                  align=\"center\"\r\n                  style={{ backgroundColor: '#ffff8d' }}\r\n                >\r\n                  <Box\r\n                    variant=\"outlined\"\r\n                    style={{\r\n                      width: larg,\r\n                      fontSize: '12px',\r\n                    }}\r\n                  >\r\n                    {row.total}\r\n                  </Box>\r\n                </StyledTableCell>\r\n              </TableRow>\r\n            ))}\r\n          </TableBody>\r\n        </Table>\r\n      </StyledTableContainer>\r\n    </div>\r\n  );\r\n  return (\r\n    <Box borderRadius={16} {...defaultProps} ml={-3} mr={-3}>\r\n      {showIgrejas}\r\n      <Modal\r\n        open={open}\r\n        onClose={handleClose}\r\n        aria-labelledby=\"simple-modal-title\"\r\n        aria-describedby=\"simple-modal-description\"\r\n      >\r\n        {body}\r\n      </Modal>\r\n    </Box>\r\n  );\r\n}\r\n"]},"metadata":{},"sourceType":"module"}