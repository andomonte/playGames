   variant=\"outlined\"\r\n              value={item[0].igreja}\r\n              disabled\r\n              size=\"small\"\r\n              className={classes.textField1}\r\n            />\r\n\r\n            <TextField\r\n              id=\"codigoIgreja\"\r\n              label=\"Código da Igreja\"\r\n              variant=\"outlined\"\r\n              value={item[0].codigoIgreja}\r\n              disabled\r\n              className={classes.textField2}\r\n              size=\"small\"\r\n            />\r\n            <TextField\r\n              id=\"data\"\r\n              label=\"Data do Relatório\"\r\n              variant=\"outlined\"\r\n              value={DataRegistrada}\r\n              disabled\r\n              className={classes.textField2}\r\n              size=\"small\"\r\n            />\r\n\r\n            <TextField\r\n              id=\"semana\"\r\n              label=\"Semana Nº:\"\r\n              variant=\"outlined\"\r\n              value={sem}\r\n              disabled\r\n              className={classes.textField3}\r\n              size=\"small\"\r\n            />\r\n          </Grid>\r\n          <Grid item xs={12} md={12} lg={12} xl={12}>\r\n            {!editar ? (\r\n              <TextField\r\n                id=\"adultos\"\r\n                label=\"Membros Adultos\"\r\n                type=\"number\"\r\n                InputLabelProps={{\r\n                  shrink: true,\r\n                }}\r\n                // {}\r\n                variant=\"outlined\"\r\n                placeholder=\"0\"\r\n              />\r\n            ) : (\r\n              { adultos }\r\n            )}\r\n\r\n            <TextField\r\n              id=\"codigoIgreja\"\r\n              label=\"Código da Igreja\"\r\n              variant=\"outlined\"\r\n              value={item[0].codigoIgreja}\r\n              disabled\r\n              className={classes.textField2}\r\n              size=\"small\"\r\n            />\r\n            <TextField\r\n              id=\"data\"\r\n              label=\"Data do Relatório\"\r\n              variant=\"outlined\"\r\n              value={DataRegistrada}\r\n              disabled\r\n              className={classes.textField2}\r\n              size=\"small\"\r\n            />\r\n\r\n            <TextField\r\n              id=\"semana\"\r\n              label=\"Semana Nº:\"\r\n              variant=\"outlined\"\r\n              value={sem}\r\n              disabled\r\n              className={classes.textField3}\r\n              size=\"small\"\r\n            />\r\n          </Grid>\r\n        </Grid>\r\n\r\n        <Button\r\n          variant=\"contained\"\r\n          color=\"primary\"\r\n          size=\"small\"\r\n          className={classes.button}\r\n          startIcon={<EditIcon />}\r\n          onClick={handleClick}\r\n          //  startIcon={<SaveIcon />}\r\n        >\r\n          Editar\r\n        </Button>\r\n      </form>\r\n    </Box>\r\n  );\r\n}\r\n\r\nexport default formulario;\r\n"]},"metadata":{},"sourceType":"module"}